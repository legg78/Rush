<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap
		PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
		"http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="transfer">
	<parameterMap id="get-transfer-queue-param-map" class="java.util.Map">
		<parameter property="container_id"/>
		<parameter property="process_id"/>
	</parameterMap>
	<select id="get-mq-url" resultClass="java.lang.String">
		SELECT param_value FROM set_parameter_value_vw WHERE name = 'MESSAGE_QUEUE_LOCATION'
	</select>
	<select id="get-bpel-url" resultClass="java.lang.String">
		SELECT param_value FROM set_parameter_value_vw WHERE name = 'BPEL_URL'
	</select>
	<select id="get-file-items-cnt" resultClass="java.lang.Long" parameterClass="java.lang.Long">
		SELECT current_count FROM prc_stat WHERE session_id=#value#
	</select>
</sqlMap>