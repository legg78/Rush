<?xml version="1.0" encoding="UTF-8"?>
<html	xmlns="http://www.w3.org/1999/xhtml"
		xmlns:f="http://java.sun.com/jsf/core"
		xmlns:h="http://java.sun.com/jsf/html"
		xmlns:ui="http://java.sun.com/jsf/facelets"
		xmlns:a4j="http://richfaces.org/a4j"
		xmlns:rich="http://richfaces.org/rich"
		xmlns:bm="http://www.bpc.ru/jsf/misc"
		xmlns:bre="http://www.bpc.ru/jsf/richfaces-ext"
		xmlns:bpct="http://www.bpc.ru/jsf/tiles"
		xmlns:c="http://java.sun.com/jstl/core">
	<ui:composition>
		<f:subview id="#{empty subviewId ? 'contextPanelView' : subviewId}">
			<a4j:queue name="queue" requestDelay="500" ignoreDupResponses="true" size="1" sizeExceededBehavior="dropNew"/>
			<h:form id="contextForm">
				<a4j:jsFunction name="refreshContextTab" eventsQueue="queue"
					requestDelay="500" limitToList="true" ajaxSingle="true"
					reRender="#{MbInstitutionContext.tabName}, contextForm, err_ajax"
					action="#{MbInstitutionContext.loadCurrentTab}"
					oncomplete="updateContextHeight('#{empty subviewId ? 'contextPanelView' : subviewId}'); ">
					<a4j:actionparam name="tabName"
						assignTo="#{MbInstitutionContext.tabName}" />
				</a4j:jsFunction>
	
				<a4j:jsFunction name="setContextTabName" reRender="contextForm, err_ajax"
					limitToList="true" ajaxSingle="true"
					oncomplete="updateContextHeight('#{empty subviewId ? 'contextPanelView' : subviewId}'); ">
					<a4j:actionparam name="tabName"
						assignTo="#{MbInstitutionContext.tabName}" />
				</a4j:jsFunction>
			</h:form>
			
			<a4j:queue name="queue" ignoreDupResponses="true" size="1" sizeExceededBehavior="dropNext" requestDelay="500" />
			<h:panelGroup styleClass="workplace" rendered="#{usession.inRole['VIEW_INSTITUTION']}" layout="block" id="workplacePanel">
				<rich:tabPanel switchType="client" id="details"  tabClass="tab-cell" style="overflow: visible;"
			    		activeTabClass="active-tab-cell" headerClass="tabs" headerSpacing="0px"
			    		contentClass="tab-content" >
					
					<rich:tab label="Details" name="detailsContextTab" onlabelclick="selectContextTab('detailsContextTab')">
	        			<f:facet name="label">
	        				<h:panelGroup styleClass="tab-label" layout="block" id="detailsContextTabLabel">
	        					<h:outputText value="#{lblCommon.details}" title="#{lblCommon.details}"/>
	        				</h:panelGroup>
	        			</f:facet>
	
						<h:panelGroup id="detailsContextTab" layout="block">
							<ui:include src="/pages/org_struct/instDetails.jspx" >
								<ui:param name="detailsBean" value="#{MbInstitutionContext}"/>
							</ui:include>
							<h:panelGroup layout="block" styleClass="bottom_search_result_block_buttons" />
						</h:panelGroup>
					</rich:tab>

				    <rich:tab label="Additional" name="flexibleFieldsContextTab" style="height:100%;" onlabelclick="selectContextTab('flexibleFieldsContextTab')">
	        			<f:facet name="label">
	        				<h:panelGroup styleClass="tab-label" layout="block">
	        					<h:outputText value="#{lblCommon.additional}" title="#{lblCommon.additional}"/>
	        				</h:panelGroup>
	        			</f:facet>
	
						<h:panelGroup id="flexibleFieldsContextTab" layout="block" styleClass="carddata-wrapper" style="height: 100%;overflow:hidden;position:relative;">
	       						<ui:include src="/pages/common/flexible/flexFieldsTableBottom.jspx">
	       								<ui:param name="flexFieldsBean" value="#{MbFlexFieldsDataContextSearch}"/>
	       								<ui:param name="parentPriv" value="#{usession.inRole['ADD_INSTITUTION'] or usession.inRole['MODIFY_INSTITUTION']}">
	       							</ui:param>
	       						</ui:include>
	       				</h:panelGroup>
				    </rich:tab>
	
	
				    <rich:tab label="Accounts" name="glAccountsContextTab" style="height:100%;" onlabelclick="selectContextTab('glAccountsContextTab')">
	       				<f:facet name="label">
	       					<h:panelGroup styleClass="first tab-label" layout="block">
	       						<h:outputText value="#{lblCommon.accounts}" title="#{lblCommon.accounts}"/>
	       					</h:panelGroup>
	       				</f:facet>
	       				<h:panelGroup id="glAccountsContextTab" layout="block" styleClass="carddata-wrapper" style="height: 100%;overflow:hidden;position:relative;">
	       						<ui:include src="/pages/accounts/list_gl_accounts_bottom.jspx">
	       							<ui:param name="glAccountsBean" value="#{MbGLAccountsContextSearch}"/>
	       						</ui:include>
	       				</h:panelGroup>
	       			</rich:tab>


				    <rich:tab label="Addresses" name="addressesContextTab" style="height:100%;" onlabelclick="selectContextTab('addressesContextTab')">
	       				<f:facet name="label">
	       					<h:panelGroup styleClass="tab-label" layout="block">
	       						<h:outputText value="#{lblCommon.addresses}" title="#{lblCommon.addresses}"/>
	       					</h:panelGroup>
	       				</f:facet>
	       				<h:panelGroup id="addressesContextTab" layout="block" styleClass="carddata-wrapper" style="height: 100%;overflow:hidden;position:relative;">
							<ui:include src="/pages/common/address/list_addresses.jspx">
								<ui:param name="parentPriv" value="#{usession.inRole['ADD_INSTITUTION'] or usession.inRole['MODIFY_INSTITUTION']}"/>
								<ui:param name="addressesBean" value="#{MbAddressesContextSearch}"/>
							</ui:include>
						</h:panelGroup>
				    </rich:tab>
	
	       			<rich:tab label="Contacts" name="contactsContextTab" style="height:100%;" onlabelclick="selectContextTab('contactsContextTab')">
	       				<f:facet name="label">
	       					<h:panelGroup styleClass="first tab-label" layout="block">
	       						<h:outputText value="#{lblCommon.contacts}" title="#{lblCommon.contacts}"/>
	       					</h:panelGroup>
	       				</f:facet>
	
	
	       				<h:panelGroup id="contactsContextTab" layout="block" styleClass="carddata-wrapper" style="height: 100%;overflow:hidden;position:relative;">
	        				<ui:include src="/pages/contacts/contactsTableBottom.jspx">
	        					<ui:param name="parentPriv" value="#{usession.inRole['ADD_INSTITUTION'] or usession.inRole['MODIFY_INSTITUTION']}"/>
	        					<ui:param name="contactsTableBean" value="#{MbContactContextSearch}"/>
	        					<ui:param name="contactsDataBean" value="#{MbContactDataContextSearch}"/>
	        				</ui:include>
	       				</h:panelGroup>
	
	       			</rich:tab>

	       			<rich:tab label="Limits" name="limitsContextTab" rendered="false" style="height:100%;" onlabelclick="selectContextTab('limitsContextTab')">
	       				<f:facet name="label">
	       					<h:panelGroup styleClass="first tab-label" layout="block">
	       						<h:outputText value="#{lblCommon.limits}" title="#{lblCommon.limits}"/>
	       					</h:panelGroup>
	       				</f:facet>
	       				<h:panelGroup id="det_lmt" layout="block" styleClass="carddata-wrapper" style="height: 100%;overflow:hidden;position:relative;">
	
	       				</h:panelGroup>
	       			</rich:tab>
	
	       			<rich:tab label="Fees" name="instFeesContextTab" rendered="false" style="height:100%;" onlabelclick="selectContextTab('instFeesContextTab')">
	       				<f:facet name="label">
	       					<h:panelGroup styleClass="tab-label" layout="block">
	       						<h:outputText value="#{lblCommon.fees}" title="#{lblCommon.fees}"/>
	       					</h:panelGroup>
	       				</f:facet>
	       				<h:panelGroup id="instFeesContextTab" layout="block" styleClass="carddata-wrapper" style="height: 100%;overflow:hidden;position:relative;">
	
	       				</h:panelGroup>
	       			</rich:tab>
	
					<rich:tab label="#{lblCommon.networks}" name="networksContextTab" style="height:100%;" onlabelclick="selectContextTab('networksContextTab')">
						<f:facet name="label">
							<h:panelGroup styleClass="first tab-label" layout="block">
								<h:outputText value="#{lblCommon.networks}"
									title="#{lblCommon.networks}" />
							</h:panelGroup>
						</f:facet>
	
						<h:panelGroup layout="block" id="networksContextTab"
							styleClass="carddata-wrapper"
							style="height: 100%;overflow:hidden;position:relative;">
							<ui:include src="/pages/net/list_net_members.jspx">
								<ui:param name="networksBean" value="#{MbNetworkMembersContext}"/>
							</ui:include>
						</h:panelGroup>
					</rich:tab>
	
					<rich:tab label="Settings" name="settingParamsContextTab" rendered="#{usession.inRole['VIEW_INST_PARAMETER']}" style="height:100%;"
						onlabelclick="selectContextTab('settingParamsContextTab')">
	       				<f:facet name="label">
	       					<h:panelGroup styleClass="first tab-label" layout="block">
	       						<h:outputText value="#{lblCommon.settings}" title="#{lblCommon.settings}"/>
	       					</h:panelGroup>
	       				</f:facet>
	       				<h:panelGroup id="settingParamsContextTab" layout="block" styleClass="carddata-wrapper" style="height: 100%;overflow:hidden;position:relative;">
	   						<ui:include src="/pages/settings/list_settings_bottom.jspx" >
	   							<ui:param name="roleToUnblock" value="MODIFY_INST_PARAMETER"></ui:param>
	   							<ui:param name="settingsBean" value="#{MbSettingParamsContextSearch}"/>
	   						</ui:include>
						</h:panelGroup>
	       			</rich:tab>

	       			<rich:tab label="Notes" name="notesContextTab" style="height:100%;" onlabelclick="selectContextTab('notesContextTab')">
	       				<f:facet name="label">
	       					<h:panelGroup styleClass="tab-label" layout="block">
	       						<h:outputText value="#{lblNote.notes}" title="#{lblNote.notes}"/>
	       					</h:panelGroup>
	       				</f:facet>
	       				<h:panelGroup id="notesContextTab" layout="block" styleClass="carddata-wrapper" style="height: 100%;overflow:hidden;position:relative;">
	       						<ui:include src="/pages/notes/list_notes_bottom.jspx">
	       							<ui:param name="notesBean" value="#{MbNotesContextSearch}"/>
	       						</ui:include>
	       				</h:panelGroup>
	       			</rich:tab>

	       			<rich:tab label="Schemes" name="schemesContextTab" style="height:100%;" onlabelclick="selectContextTab('schemesContextTab')">
	       				<f:facet name="label">
	       					<h:panelGroup styleClass="tab-label" layout="block">
	       						<h:outputText value="#{lblAup.schemes}" title="#{lblAup.schemes}"/>
	       					</h:panelGroup>
	       				</f:facet>
	       				<h:panelGroup id="schemesContextTab" layout="block" styleClass="carddata-wrapper" style="height: 100%;overflow:hidden;position:relative;">
							<ui:include src="/pages/aup/list_scheme_object.jspx">
								<ui:param name="schemeBean" value="#{MbAupSchemeObjectsContext}"/>
							</ui:include>
	       				</h:panelGroup>
	       			</rich:tab>

	       			<rich:tab label="Association" name="associationContextTab" style="height:100%;" onlabelclick="selectContextTab('associationContextTab')">
	       				<f:facet name="label">
	       					<h:panelGroup styleClass="tab-label" layout="block">
	       						<h:outputText value="#{lblPrd.association}" title="#{lblPrd.association}"/>
	       					</h:panelGroup>
	       				</f:facet>
	       				<h:panelGroup id="associationContextTab" layout="block" styleClass="carddata-wrapper" style="height: 100%;overflow:hidden;position:relative;">
							<ui:include src="/pages/context/customersListBottom.jspx">
								<ui:param name="activeItem" value="#{MbInstitution.node.id}" />
								<ui:param name="customersBean" value="#{MbCustomersBottomContext}" />
							</ui:include>
	       				</h:panelGroup>
	       			</rich:tab>

				    <rich:tab label="Agents" name="agentsContextTab" style="height:100%;" onlabelclick="selectContextTab('agentsContextTab')">
	       				<f:facet name="label">
	       					<h:panelGroup styleClass="first tab-label" layout="block">
	       						<h:outputText value="#{lblOst.agents}" title="#{lblOst.agents}"/>
	       					</h:panelGroup>
	       				</f:facet>
	       				<h:panelGroup id="agentsContextTab" layout="block" styleClass="carddata-wrapper" style="height: 100%;overflow:hidden;position:relative;">
	   						<ui:include src="/pages/org_struct/agentsBottom.jspx" >
	   							<ui:param name="agentsBean" value="#{MbAgentBottomContext}" />
	   						</ui:include>
						</h:panelGroup>
	       			</rich:tab>

			    </rich:tabPanel>
			</h:panelGroup>
				
			<script>
			//<![CDATA[
				
				updateContextHeight("#{empty subviewId ? 'contextPanelView' : subviewId}");
				makeScrollableTabs('contextMenuModalPanel\\:accountDetailsModalPanel\\:detailsContext');
			//]]>
			</script>
		</f:subview>
	</ui:composition>
</html>
