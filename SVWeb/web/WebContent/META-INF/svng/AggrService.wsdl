<?xml version="1.0" encoding="UTF-8"?>
<definitions name="sv_aggregation_wsdl"
			 xmlns="http://schemas.xmlsoap.org/wsdl/"
			 targetNamespace="http://www.bpcbt.com/sv/aggregation/service/v1/"
			 xmlns:tns="http://www.bpcbt.com/sv/aggregation/service/v1/"
			 xmlns:xsd="http://www.w3.org/2001/XMLSchema"
			 xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
			 xmlns:msg="http://www.bpcbt.com/sv/aggregation/message/v1/">

	<types>
		<schema xmlns="http://www.w3.org/2001/XMLSchema">
			<xsd:import namespace="http://www.bpcbt.com/sv/aggregation/message/v1/" schemaLocation="AggrMessages.xsd"/>
		</schema>
	</types>

	<message name="AggrRequest">
		<part name="request" element="msg:aggrRequest"/>
	</message>
	<message name="AggrResponse">
		<part name="response" element="msg:aggrResponse"/>
	</message>

	<portType name="AggrPortType">
		<operation name="Aggregate">
			<input name="AggrRequest" message="tns:AggrRequest"/>
			<output name="AggrResponse" message="tns:AggrResponse"/>
		</operation>
	</portType>

	<binding name="AggrBinding" type="tns:AggrPortType">
		<soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
		<operation name="Aggregate">
			<soap:operation soapAction="http://www.bpcbt.com/sv/aggregation/service/v1/aggregate"/>
			<input name="AggrRequest">
				<soap:body use="literal" parts="request"/>
			</input>
			<output name="AggrResponse">
				<soap:body use="literal"/>
			</output>
		</operation>
	</binding>

	<service name="AggrService">
		<port name="AggrPort" binding="tns:AggrBinding">
			<soap:address location="http://localhost:7001/aggregation"/>
		</port>
	</service>
</definitions>
