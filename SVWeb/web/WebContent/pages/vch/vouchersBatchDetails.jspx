					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblVch.proc_date}:" title="#{lblVch.proc_date}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbVouchersBatches.activeItem.procDate}"
								title="#{MbVouchersBatches.activeItem.procDate}" />
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblVch.merchant}:" title="#{lblVch.merchant}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbVouchersBatches.activeItem.merchantName}"
								title="#{MbVouchersBatches.activeItem.merchantName}" />
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblVch.terminal_number}:" title="#{lblVch.terminal_number}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbVouchersBatches.activeItem.terminalNumber}"
								title="#{MbVouchersBatches.activeItem.terminalNumber}" />
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblVch.status_reason}:" title="#{lblVch.status_reason}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{DictUtils.articles[MbVouchersBatches.activeItem.statusReason]}"
								title="#{DictUtils.articles[MbVouchersBatches.activeItem.statusReason]}" />
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblCommon.institution}:" title="#{lblCommon.institution}"/>
					</rich:column>
					<rich:column>
						<bm:entityDisplay  entityId="#{MbVouchersBatches.activeItem.instId}"
										value="#{MbVouchersBatches.activeItem.instName}"/>
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblVch.card_network}:" title="#{lblVch.card_network}"/>
					</rich:column>
					<rich:column>
						<div class="#{EntityIcons.objectsMap['ENTTNETW'][MbVouchersBatches.activeItem.cardNetworkId]}"/>
