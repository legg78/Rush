										<f:selectItem itemValue=""/>
										<f:selectItems value="#{MbEntryTemplates.balanceTypes}"/>
									</h:selectOneMenu>
									<h:outputFormat value="#{lblMsg.value_request}" styleClass="errorMarker"
											id="debitBalanceError" style="display:none;">
										<f:param name="fieldName" value="#{lblAcc.balance_type}"/>
									</h:outputFormat>
								</h:panelGrid>

								<h:outputLabel value="#{lblAcc.dest_ent_type}: " title="#{lblAcc.dest_ent_type}"/>
								<h:selectOneMenu id="debitDestEntityType"
										value="#{MbEntryTemplates.newEntryTemplatePair.debitDestEntityType}"
										disabled="#{!MbEntryTemplates.newEntryTemplatePair.editDebit or MbEntryTemplates.editMode and MbEntryTemplates.newEntryTemplatePair.debitDestEntityType!=null}">
									<a4j:support event="onchange" reRender="debitDestAccountType, dbtAccountTypeMod"
