<?xml version="1.0" encoding="UTF-8"?>
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core"
      xmlns:a4j="http://richfaces.org/a4j"
      xmlns:rich="http://richfaces.org/rich"
	  xmlns:bre="http://www.bpc.ru/jsf/richfaces-ext">
<ui:composition>
	<f:loadBundle var="lblRul" basename="ru.bpc.sv2.ui.bundles.Rul"/>
	<f:loadBundle var="lblPrd" basename="ru.bpc.sv2.ui.bundles.Prd" />
	<h:form id="attrDetailsForm">
		<h:panelGroup layout="block" styleClass="langSelector">
			<h:panelGrid columns="2" styleClass="cardholderdata oneColumnTable"
					id="detailsData" columnClasses=",secondCol">
				<h:outputLabel for="lang" value="#{lblCommon.lang_selector}: "
						title="#{lblCommon.lang_selector}" />
				<h:selectOneMenu id="lang"
						disabled="#{MbAttributes.detailNode.id == null}"
						value="#{MbAttributes.detailNode.lang}"
						valueChangeListener="#{MbAttributes.changeLanguage}">
					<a4j:support event="onchange" reRender="attrDetailsForm"
							limitToList="true" oncomplete="updateHeight();"/>
					<f:selectItems value="#{MbAttributes.languages}" />
				</h:selectOneMenu>
			</h:panelGrid>
		</h:panelGroup>
	
		<h:panelGroup id="attrDetailsWrapper" layout="block" styleClass="carddata-wrapper" 
				style="height: 100%; overflow-y: auto;overflow-x: hidden;position:relative;">
			<rich:dataTable id="attrDetailsTable"
					value="#{MbAttributes.emptyTable}" 
					var="field"
					styleClass="res-table noHeader" width="100%">
				<rich:columnGroup rowClasses=",odd" 
						columnClasses="firstCol alignLeft width20, width80 width250 noPadding">
					<rich:column styleClass="detailsCategory expandedBranch">
						<h:graphicImage url="/images/minus.gif" onclick="showHideDetails(this)"/>
						<h:outputLabel value="#{lblPrd.general_data}:" />
					</rich:column>
					<rich:column>
						<script>updateHeight();</script>
					</rich:column>					
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblCommon.id}: " title="#{lblCommon.id}" />
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbAttributes.detailNode.id}"
								title="#{MbAttributes.detailNode.id}" />
					</rich:column>
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblCommon.system_name}: " title="#{lblCommon.system_name}" />
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbAttributes.detailNode.name}"
								title="#{MbAttributes.detailNode.name}" />
					</rich:column>
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblCommon.name}: " title="#{lblCommon.name}" />
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbAttributes.detailNode.label}"
								title="#{MbAttributes.detailNode.label}" />
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblCommon.lov}: " title="#{lblCommon.lov}" />	
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbAttributes.detailNode.lovId} - #{MbAttributes.detailNode.lovName}"
								title="#{MbAttributes.detailNode.lovId} - #{MbAttributes.detailNode.lovName}"
								rendered="#{MbAttributes.detailNode.lovId != null}"/>
						<h:outputLabel value=""
							rendered="#{MbAttributes.detailNode.lovId == null}"/>
					</rich:column>
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblPrd.service_term_type}: " title="#{lblPrd.service_term_type}" />
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{DictUtils.articles[MbAttributes.detailNode.entityType]}"
								title="#{DictUtils.articles[MbAttributes.detailNode.entityType]}"
								rendered="#{MbAttributes.detailNode.entityType != null}"/>
						<h:outputLabel  
							value="#{DictUtils.articles[MbAttributes.detailNode.dataType]}"
							rendered="#{MbAttributes.detailNode.entityType == null}"/>
					</rich:column>
					
					<rich:column breakBefore="true" rendered="#{MbAttributes.currentLimit}">
						<h:outputLabel value="#{lblFcl.post_method}: " title="#{lblFcl.post_method}" />
					</rich:column>
					<rich:column rendered="#{MbAttributes.currentLimit}">
						<h:outputLabel value="#{DictUtils.articles[MbAttributes.detailNode.postMethod]}"
								title="#{DictUtils.articles[MbAttributes.detailNode.postMethod]}"
								/>
					</rich:column>				
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblCommon.object_type}: " title="#{lblCommon.object_type}" />
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{DictUtils.articles[MbAttributes.detailNode.objectType]}"
								title="#{DictUtils.articles[MbAttributes.detailNode.objectType]}"
								rendered="#{MbAttributes.detailNode.objectType != null}"/>
						<h:outputLabel value=""
								rendered="#{MbAttributes.detailNode.objectType == null}"/>					
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblCommon.display_order}: " title="#{lblCommon.display_order}" />
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbAttributes.detailNode.displayOrder}"
								title="#{MbAttributes.detailNode.displayOrder}"/>
					</rich:column>
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblPrd.def_level}: " title="#{lblPrd.def_level}" />
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{DictUtils.articles[MbAttributes.detailNode.definitionLevel]}"
								title="#{DictUtils.articles[MbAttributes.detailNode.definitionLevel]}"
								rendered="#{MbAttributes.detailNode.definitionLevel != null}"/>
						<h:outputLabel value=""
							rendered="#{MbAttributes.detailNode.definitionLevel == null}"/>	
					</rich:column>
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblPrd.service_type}: " title="#{lblPrd.service_type}" />
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbAttributes.detailNode.serviceTypeName}"
								title="#{MbAttributes.detailNode.serviceTypeName}"/>
					</rich:column>
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblCommon.description}: " title="#{lblCommon.description}" />
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbAttributes.detailNode.description}"
								title="#{MbAttributes.detailNode.description}" style="white-space: normal"/>
					</rich:column>
					
					<rich:column breakBefore="true" rendered="#{MbAttributes.currentFee || MbAttributes.currentLimit}">
						<h:outputLabel value="#{lblRul.cyclic}: " title="#{lblRul.cyclic}" />
					</rich:column>
					<rich:column rendered="#{MbAttributes.currentFee || MbAttributes.currentLimit}">
						<h:outputLabel value="#{MbAttributes.detailNode.cyclic ? 'true' : 'false'}"
								title="#{MbAttributes.detailNode.cyclic ? 'true' : 'false'}"
								rendered="#{MbAttributes.detailNode.cyclic != null}"/>
						<h:outputLabel value=""
								rendered="#{MbAttributes.detailNode.cyclic == null}"/>
					</rich:column>
					
					<rich:column breakBefore="true" rendered="#{MbAttributes.currentFee}">
						<h:outputLabel  value="#{lblRul.use_limit}: " title="#{lblRul.use_limit}" />
					</rich:column>
					<rich:column rendered="#{MbAttributes.currentFee}">
						<h:outputLabel value="#{MbAttributes.detailNode.useLimit ? 'true' : 'false'}"
								title="#{MbAttributes.detailNode.useLimit ? 'true' : 'false'}"
								rendered="#{MbAttributes.detailNode.useLimit != null}"/>
						<h:outputLabel value=""
								rendered="#{MbAttributes.detailNode.useLimit == null}"/>
					</rich:column>
					
					<rich:column breakBefore="true" rendered="#{MbAttributes.currentFee and MbAttributes.detailNode.useLimit}">
						<h:outputLabel value="#{lblRul.cyclic_limit}: " title="#{lblRul.cyclic_limit}" />
					</rich:column>
					<rich:column rendered="#{MbAttributes.currentFee and MbAttributes.detailNode.useLimit}">
						<h:outputLabel value="#{MbAttributes.detailNode.cyclicLimit ? 'true' : 'false'}"
								title="#{MbAttributes.detailNode.cyclicLimit ? 'true' : 'false'}"
								rendered="#{MbAttributes.detailNode.cyclicLimit != null}"/>
						<h:outputLabel value=""
								rendered="#{MbAttributes.detailNode.cyclicLimit == null}"/>
					</rich:column>
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblPrd.visible}: " title="#{lblPrd.visible}" />
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbAttributes.detailNode.visible ? 'true' : 'false'}"
								title="#{MbAttributes.detailNode.visible ? 'true' : 'false'}"
								rendered="#{MbAttributes.detailNode.visible != null}"/>
						<h:outputLabel value=""
								rendered="#{MbAttributes.detailNode.visible == null}"/>
					</rich:column>

					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblPrd.length_type_is_mandatory}: " title="#{lblPrd.length_type_is_mandatory}" />
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbAttributes.detailNode.lengthTypeMandatory ? 'true' : 'false'}"
									   title="#{MbAttributes.detailNode.lengthTypeMandatory ? 'true' : 'false'}"
									   rendered="#{MbAttributes.detailNode.lengthTypeMandatory != null}"/>
						<h:outputLabel value="" rendered="#{MbAttributes.detailNode.lengthTypeMandatory == null}"/>
					</rich:column>

                    <rich:column breakBefore="true" rendered="#{MbAttributes.currentLimit}">
                        <h:outputLabel  value="#{lblRul.limit_usage}: " title="#{lblRul.limit_usage}" />
                    </rich:column>
                    <rich:column rendered="#{MbAttributes.currentLimit}">
                        <h:outputLabel value="#{DictUtils.articles[MbAttributes.detailNode.limitUsage]}"
                                       title="#{DictUtils.articles[MbAttributes.detailNode.limitUsage]}"
                                       rendered="#{MbAttributes.detailNode.limitUsage != null}"/>
                        <h:outputLabel value=""
                                       rendered="#{MbAttributes.detailNode.limitUsage == null}"/>
                    </rich:column>
				</rich:columnGroup>
			</rich:dataTable>
			<script>setDetailsState("attrDetailsForm:attrDetailsTable");</script>
		</h:panelGroup>
	</h:form>
</ui:composition>
</html>
