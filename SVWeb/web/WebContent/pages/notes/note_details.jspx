<?xml version="1.0" encoding="UTF-8"?>
<html   xmlns="http://www.w3.org/1999/xhtml"
		xmlns:f="http://java.sun.com/jsf/core"
		xmlns:h="http://java.sun.com/jsf/html"
		xmlns:ui="http://java.sun.com/jsf/facelets"
		xmlns:a4j="http://richfaces.org/a4j"
		xmlns:rich="http://richfaces.org/rich"
		xmlns:bre="http://www.bpc.ru/jsf/richfaces-ext">
		
<f:view>
<ui:composition>
	
   	 <h:panelGroup layout="block" styleClass="cardholderdata-wrapper">
        	<h:panelGrid columns="2" styleClass="cardholderdata" width="100%" columnClasses="firstColModal,width250">

			<h:outputLabel value="#{lblCommon.type}: " title="#{lblCommon.type}" />
			<h:inputText id="noteType" value="#{DictUtils.articlesByLang[MbNotesSearch.activeNote.lang][MbNotesSearch.activeNote.noteType]}" readonly="true"/>
			
			<h:outputLabel value="#{lblNote.header}: " title="#{lblNote.header}" />
			<h:inputText id="noteHeader" value="#{MbNotesSearch.activeNote.header}" readonly="true"/>

			<h:outputLabel value="#{lblNote.start_date}: " title="#{lblNote.start_date}" />
			<h:inputText id="startDate" value="#{MbNotesSearch.activeNote.startDate}" readonly="true">
				<f:convertDateTime pattern="#{usession.datePattern}" timeZone="#{CommonUtils.timeZone}"/>
			</h:inputText>

			<h:outputLabel value="#{lblNote.end_date}: " title="#{lblNote.end_date}" />
			<h:inputText id="endDate" value="#{MbNotesSearch.activeNote.endDate}" readonly="true">
				<f:convertDateTime pattern="#{usession.datePattern}" timeZone="#{CommonUtils.timeZone}"/>
			</h:inputText>

			<h:outputLabel value="#{lblNote.text}: " title="#{lblNote.text}" />
			<h:inputTextarea id="noteText" value="#{MbNotesSearch.activeNote.text}" rows="5" cols="150" readonly="true"/>
			
			<h:outputLabel value="#{lblCommon.user}: " title="#{lblCommon.user}" />
			<h:inputText id="noteUser" value="#{MbNotesSearch.activeNote.userName}" readonly="true"/>		
		</h:panelGrid>
	</h:panelGroup>
</ui:composition>
</f:view>
</html>