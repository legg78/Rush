        <f:loadBundle var="lblAcc" basename="ru.bpc.sv2.ui.bundles.Acc"/>
        <f:loadBundle var="lblCst" basename="ru.bpc.sv2.ui.bundles.Cst"/>

        <h:panelGrid columns="1" cellpadjcbg="0"
                     style="width:100%; overflow-y: auto;overflow-x: hidden;position:relative;">
            <h:form id="jcbAddendumDetailsForm">
                <h:panelGroup layout="block" id="jcbAddendumDetailsWrapper"
                              style="padjcbg:0px;margin:0px;">
                    <h:outputLabel value="Addendum messages:"/>
                    <rich:extendedDataTable id="jcbAddendumDetailsTable"
                                            styleClass="res-table"
                                            headerClass="res-table-header"
                                            selectedClass="res-table-selected"
                                            var="addendum"
                                            value="#{MbJcbAddendumSearchBottom.items}"
                                            selection="#{MbJcbAddendumSearchBottom.selection}"
                                            selectionMode="single"
                                            width="100%"
                                            noDataLabel="#{MbJcbAddendumSearchBottom.searching
                                                           ? lblCommon.no_records_found
                                                           : lblCommon.enter_search_criteria_above}">
                        <a4j:support event="onselectionchange"
                                     reRender="jcbAddendumDetailsDataTable"
                                     limitToList="true" />

                        <rich:column id="id" label="#{'Id'}" width="10%">
                            <f:facet name="header">
                                <h:outputText value="#{'Id'}" title="#{'Id'}" />
                            </f:facet>
                            <h:outputText value="#{addendum.id}" />
                        </rich:column>

                        <rich:column id="fileId" label="#{'File ID'}" width="10%">
                            <f:facet name="header">
                                <h:outputText value="#{'File ID'}" title="#{'File ID'}" />
                            </f:facet>
                            <h:outputText value="#{addendum.fileId}" />
                        </rich:column>

                        <rich:column id="isIncoming" label="#{'Incoming'}" width="5%">
                            <f:facet name="header">
                                <h:outputText value="#{'Incoming'}" title="#{'Incoming'}" />
                            </f:facet>
                            <h:outputText value="#{addendum.isIncoming}" />
                        </rich:column>

                        <rich:column id="mti" label="#{'MTI'}" width="5%">
                            <f:facet name="header">
                                <h:outputText value="#{'MTI'}" title="#{'MTI'}" />
                            </f:facet>
                            <h:outputText value="#{addendum.mti}" />
                        </rich:column>

                        <rich:column id="de024" label="#{'DE024'}" width="5%">
                            <f:facet name="header">
                                <h:outputText value="#{'DE024'}" title="#{'DE024'}" />
