	  xmlns:bre="http://www.bpc.ru/jsf/richfaces-ext">
<ui:composition>
	<script type="text/javascript">//<!--
		function checkEntryTemplatesFields() {
			var checked = true;
			if (!document.getElementById("editEntryTplForm:dateNum").disabled) {
				checked = checkField("editEntryTplForm:dateNum") && checked;
			}
			if (!document.getElementById("editEntryTplForm:editDebit").checked &&
					!document.getElementById("editEntryTplForm:editCredit").checked) {
				alert("Either debit or credit or both should be checked and filled in.");
				return false;
			}
	
			checked = checkField("editEntryTplForm:transType") && checked;
			checked = checkField("editEntryTplForm:transNum") && checked;			
	
			if (document.getElementById("editEntryTplForm:editDebit").checked) {
				checked = checkField("editEntryTplForm:debitAmountName") && checked;
				checked = checkField("editEntryTplForm:debitAccount") && checked;
				checked = checkField("editEntryTplForm:debitPostMethod") && checked;
				checked = checkField("editEntryTplForm:debitBalance") && checked;
				if (document.getElementById("editEntryTplForm:debitDestEntityType").value != "" &&
						document.getElementById("editEntryTplForm:debitDestAccountType").value == "") {
					document.getElementById("editEntryTplForm:debitDestAccountTypeError").style.display = "inline";
					checked = false;
				}
			}
	
			if (document.getElementById("editEntryTplForm:editCredit").checked) {
				checked = checkField("editEntryTplForm:creditAmountName") && checked;
				checked = checkField("editEntryTplForm:creditAccount") && checked;
				checked = checkField("editEntryTplForm:creditPostMethod") && checked;
				checked = checkField("editEntryTplForm:creditBalance") && checked;
				if (document.getElementById("editEntryTplForm:creditDestEntityType").value != "" &&
						document.getElementById("editEntryTplForm:creditDestAccountType").value == "") {
					document.getElementById("editEntryTplForm:creditDestAccountTypeError").style.display = "inline";
					checked = false;
				}
			}
			return checked;
		}

		function hideDestAccountTypeError(prefix) {
			document.getElementById("editEntryTplForm:" + prefix + "DestAccountTypeError").style.display = "none";
		}
		
		function changePostingMethod(prefix) {
			otherPrefix = (prefix == 'debit') ? 'credit' : 'debit';
			otherCheckbox = (prefix == 'debit') ? 'editCredit' : 'editDebit';
			document.getElementById("editEntryTplForm:dateNumError").style.display = "none";
			if (document.getElementById("editEntryTplForm:" + prefix + "PostMethod").value == 'POSTPEND') {
				// 1. if credit or debit pending is selected, then pending must be selected in another.
				if (document.getElementById("editEntryTplForm:" + otherCheckbox).checked) {
					document.getElementById("editEntryTplForm:" + otherPrefix + "PostMethod").value = 'POSTPEND';
				}
				//
				document.getElementById("editEntryTplForm:dateNum").disabled = false;
				document.getElementById("editEntryTplForm:dateNum").value = '';				
				document.getElementById("editEntryTplForm:dateNameId").innerHTML = "* " + "#{lblAcc.date_name}:";
			} else {
				// 2. if value was changed from pending to non pending, then clear the other value
				if (document.getElementById("editEntryTplForm:" + otherPrefix + "PostMethod").value == 'POSTPEND'
						&& document.getElementById("editEntryTplForm:" + otherCheckbox).checked) {
					document.getElementById("editEntryTplForm:" + otherPrefix + "PostMethod").value = '';
				}
				//
				document.getElementById("editEntryTplForm:dateNum").value = '';
				document.getElementById("editEntryTplForm:dateNum").disabled = true;
				document.getElementById("editEntryTplForm:dateNameId").innerHTML = "#{lblAcc.date_name}:";
			}
		}
		
		function initDateNum() {
			var postMethod = null;
			if (!document.getElementById("editEntryTplForm:creditPostMethod").disabled) {
				postMethod = document.getElementById("editEntryTplForm:creditPostMethod");
			} else {
				postMethod = document.getElementById("editEntryTplForm:debitPostMethod");
			}
			if (postMethod.value == 'POSTPEND') {
				document.getElementById("editEntryTplForm:dateNum").disabled = false;
				if (document.getElementById("editEntryTplForm:dateNum").value == null){
					document.getElementById("editEntryTplForm:dateNum").value = '';
				}
				document.getElementById("editEntryTplForm:dateNameId").innerHTML = "* " + "#{lblAcc.date_name}:";
			} else {
				document.getElementById("editEntryTplForm:dateNum").value = null;
				document.getElementById("editEntryTplForm:dateNum").disabled = true;
				document.getElementById("editEntryTplForm:dateNameId").innerHTML = "#{lblAcc.date_name}:";
			}
		}
		
	//-->
	</script>
	<rich:modalPanel id="entryTmplModalPanel" autosized="true" minWidth="300" minHeight="100"
			onshow="document.getElementById('editEntryTplForm:transType').focus()">
 	    <f:facet name="header">
 		    <h:panelGroup id="entryTmplModalHeader">
           	    <h:outputText value="#{lblAcc.new_transaction_template}" rendered="#{MbEntryTemplates.newMode}"/>
           	    <h:outputText value="#{lblAcc.edit_transaction_template}" rendered="#{MbEntryTemplates.editMode}"/>
       	    </h:panelGroup>
        </f:facet>
  	    <f:facet name="controls">
      	</f:facet>
      	<h:panelGroup id="entryTmplModalDiv">
      		<h:form id="editEntryTplForm">
				<h:panelGroup layout="block" styleClass="cardholderdata-wrapper">
			        	<h:panelGrid columns="2" styleClass="cardholderdata" width="100%" columnClasses="firstColModal, width250">
							<h:outputLabel value="#{lblAcc.bunch_type}: " title="#{lblAcc.bunch_type}"/>
							<h:inputText value="#{MbEntryTemplates.bunchTypeName}" readonly="true"
									styleClass="grayInput" style="width:99%"/>

							<h:outputLabel value="* #{lblAcc.trans_type}: " title="#{lblAcc.trans_type}"/>
							<h:panelGrid columns="1" cellspacing="0" cellpadding="0" style="width:100%">
								<h:selectOneMenu id="transType"
										value="#{MbEntryTemplates.newEntryTemplatePair.transactionType}"
										style="width:100%"
										onblur="hideErrorField(this)">
									<f:selectItem itemValue=""/>
									<f:selectItems value="#{MbEntryTemplates.transactionTypes}"/>
								</h:selectOneMenu>
								<h:outputFormat value="#{lblMsg.value_request}" styleClass="errorMarker"
										id="transTypeError" style="display:none;">
									<f:param name="fieldName" value="#{lblAcc.trans_type}"/>
								</h:outputFormat>
						    </h:panelGrid>

							<h:outputLabel value="* #{lblAcc.trans_num}: " title="#{lblAcc.trans_num}"/>
							<h:panelGrid columns="1" cellspacing="0" cellpadding="0" style="width:100%">
								<h:inputText id="transNum"
										value="#{MbEntryTemplates.newEntryTemplatePair.transactionNum}"
										maxlength="4" style="width:99%"
										onkeypress="if (!numbersOnly(this, event)) return false; "
										onblur="hideErrorField(this);">
								</h:inputText>
								<h:outputFormat value="#{lblMsg.value_request}" styleClass="errorMarker"
										id="transNumError" style="display:none;">
									<f:param name="fieldName" value="#{lblAcc.trans_num}"/>
								</h:outputFormat>
							</h:panelGrid>

							<h:outputLabel value="* #{lblAcc.date_name}: " title="#{lblAcc.date_name}" id="dateNameId"/>
							<h:panelGrid columns="1" cellspacing="0" cellpadding="0" style="width:100%">
								<h:selectOneMenu id="dateNum"
										value="#{MbEntryTemplates.newEntryTemplatePair.dateName}"
										style="width:100%"
										onblur="hideErrorField(this)">
									<f:selectItem itemValue = ""/>
									<f:selectItems value="#{MbEntryTemplates.datePurposes}"/>
								</h:selectOneMenu>
								<h:outputFormat value="#{lblMsg.value_request}" styleClass="errorMarker"
										id="dateNumError" style="display:none;">
									<f:param name="fieldName" value="#{lblAcc.date_name}"/>
								</h:outputFormat>
							</h:panelGrid>

							<h:outputLabel value="#{lblAcc.negative_allowed}" title="#{lblAcc.negative_allowed}"/>
							<h:selectBooleanCheckbox value="#{MbEntryTemplates.newEntryTemplatePair.negativeAllowed}"/>
						</h:panelGrid>

						<h:panelGrid columns="2" id="pairTable" style="width:100%;" columnClasses="top,top">
							<f:facet name="header">
								<h:panelGrid columns="2" style="width:100%">
									<h:panelGroup>
										<h:selectBooleanCheckbox
												value="#{MbEntryTemplates.newEntryTemplatePair.editDebit}"
