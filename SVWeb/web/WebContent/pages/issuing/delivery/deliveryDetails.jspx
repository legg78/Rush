					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblIss.product}:" title="#{lblIss.product}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel rendered="#{MbDeliveries.activeDelivery.productId != null}"
									   value="#{MbDeliveries.activeDelivery.productId} - #{MbDeliveries.activeDelivery.productName}"
									   title="#{MbDeliveries.activeDelivery.productId} - #{MbDeliveries.activeDelivery.productName}"/>
						<h:outputLabel rendered="#{MbDeliveries.activeDelivery.productId == null}" value="" title=""/>
					</rich:column>

                    <rich:column breakBefore="true">
                        <h:outputLabel value="#{lblCommon.company_name}:" title="#{lblCommon.company_name}"/>
                    </rich:column>
                    <rich:column>
                        <h:outputLabel value="#{MbDeliveries.activeDelivery.companyName}"
