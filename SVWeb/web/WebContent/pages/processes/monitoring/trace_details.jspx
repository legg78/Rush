<?xml version="1.0" encoding="UTF-8"?>
<html	xmlns="http://www.w3.org/1999/xhtml"
		xmlns:f="http://java.sun.com/jsf/core"
		xmlns:h="http://java.sun.com/jsf/html"
		xmlns:ui="http://java.sun.com/jsf/facelets"
		xmlns:a4j="http://richfaces.org/a4j"
		xmlns:rich="http://richfaces.org/rich"
		xmlns:bm="http://www.bpc.ru/jsf/misc"
		xmlns:bre="http://www.bpc.ru/jsf/richfaces-ext">
<ui:composition>
	<h:panelGroup layout="block" styleClass="cardholderdata-wrapper">
		<h:panelGrid columns="2" styleClass="cardholderdata oneColumnTable" width="100%" columnClasses="firstColModal,secondCol">        
      	
			<h:outputLabel for="name" value="#{lblProcess.thread}: " title="#{lblProcess.thread}" />
			<h:inputText id="name" value="#{MbProcessTrace.activeProcessTrace.threadNumber}" readonly="true"/>
			
			<h:outputLabel for="full_desc" value="#{lblProcess.trace_level}: " title="#{lblProcess.trace_level}" />
			<h:inputText id="full_desc" value="#{MbProcessTrace.activeProcessTrace.traceLevel}" readonly="true"/>
			
			<h:outputLabel for="entity_type" value="#{lblCommon.entity_type}: " title="#{lblCommon.entity_type}" />
			<h:inputText id="entity_type" value="#{DictUtils.articles[MbProcessTrace.activeProcessTrace.entityType]}" readonly="true"/>
			
			<h:outputLabel for="object_id" value="#{lblCommon.object_id}: " title="#{lblCommon.object_id}" />
			<h:inputText id="object_id" value="#{MbProcessTrace.activeProcessTrace.objectId} - #{MbProcessTrace.activeProcessTrace.entityDescription}" readonly="true"/>
			
			<h:outputLabel value="#{lblProcess.trace_timestamp}: " title="#{lblProcess.trace_timestamp}" />
			<h:inputText value="#{MbProcessTrace.activeProcessTrace.traceTimestamp}">
				<f:convertDateTime pattern="#{usession.datePattern} HH:mm:ss:SSS" timeZone="#{CommonUtils.timeZone}" />
			</h:inputText>
			
			<h:outputLabel for="trace_text" value="#{lblProcess.trace_text}: " title="#{lblProcess.trace_text}" />
			<h:inputTextarea id="trace_text" rows="10" value="#{MbProcessTrace.activeProcessTrace.traceText}" readonly="true" style="width:550px;max-width:550px"/>
		</h:panelGrid>		
	</h:panelGroup>
</ui:composition>
</html>