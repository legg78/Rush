    <xs:complexType name="Pdc">
        <xs:sequence>
            <xs:element name="pdc_1" type="String32" minOccurs="0" maxOccurs="1"/>
            <xs:element name="pdc_2" type="String32" minOccurs="0" maxOccurs="1"/>
