<?xml version="1.0" encoding="UTF-8"?>
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:f="http://java.sun.com/jsf/core"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:rich="http://richfaces.org/rich">
<ui:composition >
    <rich:column breakBefore="true">
        <h:outputLabel value="#{lblCommon.surname}:" title="#{lblCommon.surname}"/>
    </rich:column>
    <rich:column>
        <h:outputLabel value="#{(cardholderBean==null ? MbCardholdersSearch : cardholderBean).activeCardholder.person.surname}"
                       title="#{(cardholderBean==null ? MbCardholdersSearch : cardholderBean).activeCardholder.person.surname}"/>
    </rich:column>

    <rich:column breakBefore="true">
        <h:outputLabel value="#{lblCommon.name}:" title="#{lblCommon.name}"/>
    </rich:column>
    <rich:column>
        <h:outputLabel value="#{(cardholderBean==null ? MbCardholdersSearch : cardholderBean).activeCardholder.person.firstName}"
                       title="#{(cardholderBean==null ? MbCardholdersSearch : cardholderBean).activeCardholder.person.firstName}" />
    </rich:column>

    <rich:column breakBefore="true">
        <h:outputLabel value="#{lblCommon.second_name}:" title="#{lblCommon.second_name}"/>
    </rich:column>
    <rich:column>
        <h:outputLabel value="#{(cardholderBean==null ? MbCardholdersSearch : cardholderBean).activeCardholder.person.secondName}"
                       title="#{(cardholderBean==null ? MbCardholdersSearch : cardholderBean).activeCardholder.person.secondName}" />
    </rich:column>

    <rich:column breakBefore="true">
        <h:outputLabel value="#{lblCommon.birthday}:" title="#{lblCommon.birthday}"/>
    </rich:column>
    <rich:column>
        <h:outputLabel value="#{(cardholderBean==null ? MbCardholdersSearch : cardholderBean).activeCardholder.person.birthday}"
                       title="#{(cardholderBean==null ? MbCardholdersSearch : cardholderBean).activeCardholder.person.birthday}" >
            <f:convertDateTime pattern="#{usession.datePattern}" timeZone="#{CommonUtils.timeZone}" />
        </h:outputLabel>
    </rich:column>



    <rich:column breakBefore="true">
        <h:outputLabel value="#{lblCommon.place_of_birth}:" title="#{lblCommon.place_of_birth}"/>
    </rich:column>
    <rich:column>
        <h:outputLabel value="#{(cardholderBean==null ? MbCardholdersSearch : cardholderBean).activeCardholder.person.placeOfBirth}"
                       title="#{(cardholderBean==null ? MbCardholdersSearch : cardholderBean).activeCardholder.person.placeOfBirth}" />
    </rich:column>

    <rich:column breakBefore="true">
        <h:outputLabel value="#{lblCommon.suffix}:" title="#{lblCommon.suffix}"/>
    </rich:column>
    <rich:column>
        <h:outputLabel value="#{DictUtils.articlesByLang[(cardholderBean==null ? MbCardholdersSearch : cardholderBean).curLang][(cardholderBean==null ? MbCardholdersSearch : cardholderBean).activeCardholder.person.suffix]}"
                       title="#{DictUtils.articlesByLang[(cardholderBean==null ? MbCardholdersSearch : cardholderBean).curLang][(cardholderBean==null ? MbCardholdersSearch : cardholderBean).activeCardholder.person.suffix]}" />
    </rich:column>

    <rich:column breakBefore="true">
        <h:outputLabel value="#{lblCommon.gender}:" title="#{lblCommon.gender}"/>
    </rich:column>
    <rich:column>
        <h:outputLabel value="#{DictUtils.articlesByLang[(cardholderBean==null ? MbCardholdersSearch : cardholderBean).curLang][(cardholderBean==null ? MbCardholdersSearch : cardholderBean).activeCardholder.person.gender]}"
                       title="#{DictUtils.articlesByLang[(cardholderBean==null ? MbCardholdersSearch : cardholderBean).curLang][(cardholderBean==null ? MbCardholdersSearch : cardholderBean).activeCardholder.person.gender]}" />
    </rich:column>

    <rich:column breakBefore="true">
        <h:outputLabel value="#{lblPrd.relation}:" title="#{lblPrd.relation}"/>
    </rich:column>
    <rich:column>
        <h:outputLabel rendered="#{(cardholderBean==null ? MbCardholdersSearch : cardholderBean).activeCardholder.relation != null}"
                       value="#{DictUtils.articlesByLang[(cardholderBean==null ? MbCardholdersSearch : cardholderBean).curLang][(cardholderBean==null ? MbCardholdersSearch : cardholderBean).activeCardholder.relation]}"
                       title="#{DictUtils.articlesByLang[(cardholderBean==null ? MbCardholdersSearch : cardholderBean).curLang][(cardholderBean==null ? MbCardholdersSearch : cardholderBean).activeCardholder.relation]}"
        />
        <h:outputLabel value="" rendered="#{(cardholderBean==null ? MbCardholdersSearch : cardholderBean).activeCardholder.relation == null}" />
    </rich:column>

    <rich:column breakBefore="true">
        <h:outputLabel value="#{lblPrd.nationality}:" title="#{lblPrd.nationality}"/>
    </rich:column>
    <rich:column>
        <ui:remove><div class="#{CountryUtils.flag[(cardholderBean==null ? MbCardholdersSearch : cardholderBean).activeCardholder.nationality]}" /></ui:remove>
        <div class="#{EntityIcons.objectsMapS['ENTTACCT'][(cardholderBean==null ? MbCardholdersSearch : cardholderBean).activeCardholder.nationality]}" />
        <h:outputLabel
                value="#{CountryUtils.countryNamesMap[(cardholderBean==null ? MbCardholdersSearch : cardholderBean).activeCardholder.nationality]}"
                title="#{CountryUtils.countryNamesMap[(cardholderBean==null ? MbCardholdersSearch : cardholderBean).activeCardholder.nationality]}"
                rendered="#{(cardholderBean==null ? MbCardholdersSearch : cardholderBean).activeCardholder.nationality != null}" />
        <h:outputLabel value="" rendered="#{(cardholderBean==null ? MbCardholdersSearch : cardholderBean).activeCardholder.nationality == null}" />
    </rich:column>

    <rich:column breakBefore="true">
        <h:outputLabel value="#{lblPrd.resident}:" title="#{lblPrd.resident}"/>
    </rich:column>
    <rich:column>
        <h:outputLabel value="#{(cardholderBean==null ? MbCardholdersSearch : cardholderBean).activeCardholder.resident ? lblCommon.yes : lblCommon.no}"
                       title="#{(cardholderBean==null ? MbCardholdersSearch : cardholderBean).activeCardholder.resident ? lblCommon.yes : lblCommon.no}"
                       rendered="#{(cardholderBean==null ? MbCardholdersSearch : cardholderBean).activeCardholder.resident != null}"/>
    </rich:column>

    <rich:column breakBefore="true">
        <h:outputLabel value="#{lblPrd.marital_status}:" title="#{lblPrd.marital_status}"/>
    </rich:column>
    <rich:column>
        <h:outputLabel value="#{DictUtils.articlesByLang[(cardholderBean==null ? MbCardholdersSearch : cardholderBean).curLang][(cardholderBean==null ? MbCardholdersSearch : cardholderBean).activeCardholder.maritalStatus]}"
                       title="#{DictUtils.articlesByLang[(cardholderBean==null ? MbCardholdersSearch : cardholderBean).curLang][(cardholderBean==null ? MbCardholdersSearch : cardholderBean).activeCardholder.maritalStatus]}" />
    </rich:column>
</ui:composition>
</html>

