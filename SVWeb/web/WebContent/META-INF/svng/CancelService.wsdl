<?xml version="1.0" encoding="UTF-8"?>
<definitions name="sv2_svfe_wsdl"
			 xmlns="http://schemas.xmlsoap.org/wsdl/"
			 targetNamespace="http://www.bpcbt.com/sv/cancel/service/v1/"
			 xmlns:tns="http://www.bpcbt.com/sv/cancel/service/v1/"
			 xmlns:xsd="http://www.w3.org/2001/XMLSchema"
			 xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
			 xmlns:msg="http://www.bpcbt.com/sv/cancel/message/v1/">

	<types>
		<schema xmlns="http://www.w3.org/2001/XMLSchema">
			<xsd:import namespace="http://www.bpcbt.com/sv/cancel/message/v1/" schemaLocation="CancelMessages.xsd"/>
		</schema>
	</types>

	<message name="CancelNotificationRequest">
		<part name="request" element="msg:CancelNotificationRequest"/>
	</message>

	<portType name="CancelPortType">
		<operation name="CancelNotification">
			<input name="CancelNotificationRequest" message="tns:CancelNotificationRequest"/>
		</operation>
	</portType>

	<binding name="CancelBinding" type="tns:CancelPortType">
		<soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
		<operation name="CancelNotification">
			<soap:operation soapAction="http://www.bpcbt.com/sv/svfe/service/v1/CancelNotification"/>
			<input name="CancelNotificationRequest">
				<soap:body use="literal"/>
			</input>
		</operation>
	</binding>

	<service name="CancelService">
		<port name="CancelPort" binding="tns:CancelBinding">
			<soap:address location="http://localhost:7001/svfe"/>
		</port>
	</service>
</definitions>
