<?xml version="1.0" encoding="UTF-8"?>
<html xmlns="http://www.w3.org/1999/xhtml"
	  xmlns:f="http://java.sun.com/jsf/core"
	  xmlns:h="http://java.sun.com/jsf/html"
	  xmlns:ui="http://java.sun.com/jsf/facelets"
	  xmlns:a4j="http://richfaces.org/a4j"
	  xmlns:rich="http://richfaces.org/rich"
	  xmlns:bm="http://www.bpc.ru/jsf/misc"
	  xmlns:bre="http://www.bpc.ru/jsf/richfaces-ext"
	  xmlns:bpct="http://www.bpc.ru/jsf/tiles">
<ui:composition>
	<script type="text/javascript">
//<!--
		function checkrevenueSharingFields() {
			var checked = true;

			if (document.getElementById('revenueSharingModalDiv:terminalIdHidden').value == "") {
				document.getElementById('revenueSharingModalDiv:newTerminalError').style.display = "inline";
				checked = false;
			}
			
			if (document.getElementById('revenueSharingModalDiv:customerIdHidden').value == "") {
				document.getElementById('revenueSharingModalDiv:newCustomerError').style.display = "inline";
				checked = false;
			}
			
			if (document.getElementById('revenueSharingModalDiv:newAccount').value == "") {
				document.getElementById('revenueSharingModalDiv:newAccountError').style.display = "inline";
				checked = false;
			}
			
			if (document.getElementById('revenueSharingModalDiv:newFeeType').value == "") {
				document.getElementById('revenueSharingModalDiv:newFeeTypeError').style.display = "inline";
				checked = false;
			}

			if (document.getElementById('revenueSharingModalDiv:newFeeId').value == "") {
				document.getElementById('revenueSharingModalDiv:newFeeIdError').style.display = "inline";
				checked = false;
			}
			
			return checked;
		}
		//-->
	</script>

	<f:loadBundle var="lblAcq" basename="ru.bpc.sv2.ui.bundles.Acq" />
	<f:loadBundle var="lblAcc" basename="ru.bpc.sv2.ui.bundles.Acc" />
	<f:loadBundle var="lblFcl" basename="ru.bpc.sv2.ui.bundles.Fcl" />
	<f:loadBundle var="lblPrd" basename="ru.bpc.sv2.ui.bundles.Prd"/>
	<f:loadBundle var="lblPmo" basename="ru.bpc.sv2.ui.bundles.Pmo"/>

	<rich:modalPanel id="revenueSharingModalPanel" autosized="true" minWidth="300" minHeight="100">
		<f:facet name="header">
			<h:panelGroup id="revenueSharingModalHeader">
				<h:outputText value="#{lblAcq.edit_revenue_sharing}" rendered="#{MbRevenueSharing.editMode}" />
				<h:outputText value="#{lblAcq.add_revenue_sharing}" rendered="#{MbRevenueSharing.newMode}" />
			</h:panelGroup>
		</f:facet>
		<f:facet name="controls">
		</f:facet>
		
		<h:form id="revenueSharingModalDiv">
			<h:panelGroup layout="block" styleClass="cardholderdata-wrapper">
				<a4j:region>
				<h:panelGrid columns="2" styleClass="cardholderdata" width="100%"
					columnClasses="firstColModal nowrap top, width250">

					<h:outputLabel value="* #{lblAcq.terminal}: " title="#{lblAcq.terminal}" />
					<h:panelGrid columns="1" cellspacing="0" cellpadding="0">
						<bm:selectTerminal valueId="#{MbRevenueSharing.newRevenueSharing.terminalId}"
							valueNumber="#{MbRevenueSharing.newRevenueSharing.terminalNumber}"
							custInfoField="#{MbRevenueSharing.newRevenueSharing.terminalName}" 
							beanName="MbRevenueSharing"
							methodName="selectNewTerminal"
							modalPanel="terminalSearchModalPanel" 
							formName="revenueSharingModalDiv"
							readonly="true"
							style="margin-left:0px;"
							searchOnBlur="false"/>						
						<h:outputFormat value="#{lblMsg.value_request}" styleClass="errorMarker" id="newTerminalError"
							style="display:none;">
							<f:param name="fieldName" value="#{lblAcq.terminal}" />
						</h:outputFormat>
					</h:panelGrid>

					<h:outputLabel for="customerNumber" value="* #{lblPrd.customer}: " title="#{lblPrd.customer}" />
					<h:panelGrid columns="1" cellspacing="0" cellpadding="0">
						<bm:selectCustomer id="customerNumber"
										   valueId="#{MbRevenueSharing.newRevenueSharing.customerId}"
										   valueNumber="#{MbRevenueSharing.newRevenueSharing.customerNumber}"
										   custInfoField="#{MbRevenueSharing.newRevenueSharing.customerName}"
										   beanName="MbRevenueSharing"
										   methodName="selectNewCustomer"
										   modalPanel="customerSearchModalPanel"
										   formName="revenueSharingModalDiv"
										   readonly="false"
										   clearOnFocus="false"
										   style="width:100%; margin-left:0px; margin-right: 20px;"
										   rerenderList="revenueSharingModalDiv:newAccount, revenueSharingModalDiv:newFeeId"
										   searchOnBlur="false"
										   manualInput="true"
										   disabled="false"
										   rendered="true"/>
						<h:outputFormat id="newCustomerError" value="#{lblMsg.value_request}"
										styleClass="errorMarker" style="display:none;">
							<f:param name="fieldName" value="#{lblPrd.customer}" />
						</h:outputFormat>
					</h:panelGrid>
					
					<h:outputLabel value="* #{lblAcc.account}: " title="#{lblAcc.account}" />
					<h:panelGrid columns="1" cellspacing="0" cellpadding="0">
						<h:selectOneMenu id="newAccount" value="#{MbRevenueSharing.newRevenueSharing.accountId}"
							label="#{lblAcc.account}" onblur="hideErrorField(this)">
							<f:selectItem itemValue="" />
							<f:selectItems value="#{MbRevenueSharing.customerAccounts}" />
						</h:selectOneMenu>
						<h:outputFormat value="#{lblMsg.value_request}" styleClass="errorMarker" id="newAccountError"
							style="display:none;">
							<f:param name="fieldName" value="#{lblAcc.account}" />
						</h:outputFormat>
					</h:panelGrid>

					<h:outputLabel value="* #{lblFcl.fee_type}: " title="#{lblFcl.fee_type}" />
					<h:panelGrid columns="1" cellspacing="0" cellpadding="0">
						<h:selectOneMenu id="newFeeType" value="#{MbRevenueSharing.newRevenueSharing.feeType}"
							label="#{lblFcl.fee_type}" onblur="hideErrorField(this)">
							<f:selectItem itemValue="" itemLabel="" />
							<f:selectItems value="#{MbRevenueSharing.feeTypes}" />
							<a4j:support event="onchange" reRender="newFeeId" limitToList="true" ajaxSingle="true"/>
						</h:selectOneMenu>
						<h:outputFormat value="#{lblMsg.value_request}" styleClass="errorMarker"
							id="newFeeTypeError" style="display:none;">
							<f:param name="fieldName" value="#{lblFcl.fee_type}" />
						</h:outputFormat>
					</h:panelGrid>

					<h:outputLabel value="* #{lblFcl.fee}: " title="#{lblFcl.fee}" />
					<h:panelGrid columns="1" cellspacing="0" cellpadding="0">
						<h:selectOneMenu id="newFeeId"
										 value="#{MbRevenueSharing.newRevenueSharing.feeId}"
										 label="#{lblFcl.fee}"
										 onblur="hideErrorField(this)">
							<f:selectItem itemValue="" itemLabel="" />
							<f:selectItems value="#{MbRevenueSharing.fees}" />
							<a4j:support event="onchange" reRender="newFeeId, newProviderId, newModifierId"
										 limitToList="true" ajaxSingle="true"/>
						</h:selectOneMenu>
						<h:outputFormat id="newFeeIdError" value="#{lblMsg.value_request}"
										styleClass="errorMarker" style="display:none;">
							<f:param name="fieldName" value="#{lblFcl.fee}" />
						</h:outputFormat>
					</h:panelGrid>
					
					<h:outputLabel value="#{lblPmo.provider}: " title="#{lblPmo.provider}" />
					<h:panelGrid columns="1" cellspacing="0" cellpadding="0">
						<h:selectOneMenu id="newProviderId" value="#{MbRevenueSharing.newRevenueSharing.providerId}"
							label="#{lblPmo.provider}">
							<f:selectItem itemValue="" itemLabel="" />
							<f:selectItems value="#{MbRevenueSharing.providers}" />
							<a4j:support event="onchange" reRender="newProviderId, newModifierId"
										 limitToList="true" ajaxSingle="true"/>
						</h:selectOneMenu>
					</h:panelGrid>
					
					<h:outputLabel value="#{lblAcq.modifier}: " title="#{lblAcq.modifier}" />
					<h:panelGrid columns="1" cellspacing="0" cellpadding="0">
						<h:selectOneMenu id="newModifierId" value="#{MbRevenueSharing.newRevenueSharing.modifierId}"
							label="#{lblAcq.modifier}">
							<f:selectItem itemValue="" itemLabel="" />
							<f:selectItems value="#{MbRevenueSharing.modifiers}" />
							<a4j:support event="onchange" reRender="newModifierId" limitToList="true" ajaxSingle="true"/>
						</h:selectOneMenu>
					</h:panelGrid>

				</h:panelGrid>
				</a4j:region>
			</h:panelGroup>

			<h:panelGroup layout="block" styleClass="bottom_search_result_block_buttons">
				<h:panelGroup layout="block" styleClass="bottom_search_result_left_buttons">
					<h:panelGrid columns="3" columnClasses="width99P,,paddingR12">
						<h:outputText />
						<bre:taggedCommandButton value="#{lblForm.save}" type="submit"
												 action="#{MbRevenueSharing.save}"
												 onclick="if (!checkrevenueSharingFields())
												          return false;disableModalPanelBtns(this)"
												 oncomplete="enableModalPanelBtns(this);
												             if (#{usession.noErrorResponse}) #{rich:component('revenueSharingModalPanel')}.hide()"
												 reRender="${rerenderList}"
												 rendered="#{!MbRevenueSharing.viewMode}" />
						<bre:taggedCommandButton value="#{lblForm.cancel}" immediate="true"
												 action="#{MbRevenueSharing.cancel}"
												 oncomplete="#{rich:component('revenueSharingModalPanel')}.hide()">
							<f:actionListener type="ru.bpc.jsf.A4JFormReset" />
						</bre:taggedCommandButton>
					</h:panelGrid>
				</h:panelGroup>
			</h:panelGroup>
		</h:form>
	</rich:modalPanel>

</ui:composition>
</html>
