            <element name="network_currency" type="tns:currency" minOccurs="0" maxOccurs="1"/>
            <element name="network_cnvt_date" type="dateTime" minOccurs="0" maxOccurs="1"/>
            <element name="network_cnvt_rate" minOccurs="0" maxOccurs="1">
