<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:ui="http://java.sun.com/jsf/facelets">
	<h:panelGroup>
		<h:panelGroup>
			<h:selectBooleanCheckbox id="nullsFirst"
				value="#{editReportTemplate.nullsFirst}"
				onclick="#{rich:element('nullsLast')}.checked=false;" />
			<h:outputLabel value="#{lblRptc.nulls_first}" for="nullsFirst" />
		</h:panelGroup>
		<br />
		<h:panelGroup>
			<h:selectBooleanCheckbox id="nullsLast"
				value="#{editReportTemplate.nullsLast}"
				onclick="#{rich:element('nullsFirst')}.checked=false;" />
			<h:outputLabel value="#{lblRptc.nulls_last}" for="nullsLast" />
		</h:panelGroup>
	</h:panelGroup>
</ui:composition>
