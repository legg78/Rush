<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:a4j="http://richfaces.org/a4j">
	<h:selectManyListbox
		value="#{editReportTemplate.sortingParams.sourceSelection}"
		converter="dynamicReportInnerListConverter" size="20">
		<f:selectItems value="#{editReportTemplate.sortingParams.sourceValue}"
			var="item" itemLabel="#{item.label}" />
		<a4j:support event="onchange" reRender="copyButtons" />
	</h:selectManyListbox>
</ui:composition>
