<?xml version="1.0" encoding="UTF-8"?>
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core"
      xmlns:a4j="http://richfaces.org/a4j"
      xmlns:rich="http://richfaces.org/rich"
	  xmlns:bre="http://www.bpc.ru/jsf/richfaces-ext">
<ui:composition>
	<h:form id="fraudDataDetailsForm">
	    <h:panelGroup layout="block" styleClass="cardholderdata-wrapper">
	        <h:panelGrid columns="4" styleClass="cardholderdata twoColumnTable" width="100%" columnClasses="firstCol,secondCol,firstCol,secondCol">
				<h:outputLabel value="#{lblFrp.auth_id}: " title="#{lblFrp.auth_id}" />
				<h:inputText readonly="true" 
						value="#{MbFraudData.activeFraudData.authId}"
						title="#{MbFraudData.activeFraudData.authId}"/>
				
				<h:outputLabel value="#{lblCommon.entity_type}: " title="#{lblCommon.entity_type}" />
				<h:inputText id="entityType" readonly="true" 
						value="#{DictUtils.articles[MbFraudData.activeFraudData.entityType]}"
						title="#{DictUtils.articles[MbFraudData.activeFraudData.entityType]}"
						rendered="#{MbFraudData.activeFraudData != null}"
						/>
				<h:inputText readonly="true" value=""
						rendered="#{MbFraudData.activeFraudData == null}"
						/>

				<h:outputLabel value="#{lblFrp.oper_date}: " title="#{lblFrp.oper_date}" />
				<h:inputText readonly="true"
						value="#{MbFraudData.activeFraudData.operDate}"
						title="#{MbFraudData.activeFraudData.operDate}">
					<f:convertDateTime pattern="#{usession.datePattern}" timeZone="#{CommonUtils.timeZone}"/>
				</h:inputText>

				<h:outputLabel value="#{lblCommon.object_id}: " title="#{lblCommon.object_id}" />
				<h:inputText readonly="true" 
						value="#{MbFraudData.activeFraudData.objectId}"
						title="#{MbFraudData.activeFraudData.objectId}"/>
				
				<h:outputLabel value="#{lblOpr.msg_type}: " title="#{lblOpr.msg_type}" />
				<h:inputText readonly="true" 
						value="#{DictUtils.articles[MbFraudData.activeFraudData.msgType]}"
						title="#{DictUtils.articles[MbFraudData.activeFraudData.msgType]}"
						rendered="#{MbFraudData.activeFraudData.msgType != null}"
						/>
				<h:inputText readonly="true" value=""
						rendered="#{MbFraudData.activeFraudData.msgType == null}"
						/>

				<h:outputLabel value="#{lblCommon.event_type}: " title="#{lblCommon.event_type}" />
				<h:inputText readonly="true" 
						value="#{DictUtils.articles[MbFraudData.activeFraudData.eventType]}"
						title="#{DictUtils.articles[MbFraudData.activeFraudData.eventType]}"
						rendered="#{MbFraudData.activeFraudData.eventType != null}"
						/>
				<h:inputText readonly="true" value=""
						rendered="#{MbFraudData.activeFraudData.eventType == null}"
						/>

				<h:outputLabel value="#{lblIss.card_number}: " title="#{lblIss.card_number}" />
				<h:inputText readonly="true" 
						value="#{MbFraudData.activeFraudData.cardNumber}"
						title="#{MbFraudData.activeFraudData.cardNumber}"/>

				<h:outputLabel value="#{lblAcq.merchant_number}: " title="#{lblAcq.merchant_number}" />
				<h:inputText readonly="true" 
						value="#{MbFraudData.activeFraudData.merchantNumber}"
						title="#{MbFraudData.activeFraudData.merchantNumber}"/>

				<h:outputLabel value="#{lblAcq.terminal_number}: " title="#{lblAcq.terminal_number}" />
				<h:inputText readonly="true" 
						value="#{MbFraudData.activeFraudData.terminalNumber}"
						title="#{MbFraudData.activeFraudData.terminalNumber}"/>

				<h:outputLabel value="#{lblFrp.serial_number}: " title="#{lblFrp.serial_number}" />
				<h:inputText readonly="true" 
						value="#{MbFraudData.activeFraudData.serialNumber}"
						title="#{MbFraudData.activeFraudData.serialNumber}"/>

				<h:outputLabel value="#{lblOpr.operation_amount}: " title="#{lblOpr.operation_amount}" />
				<h:inputText readonly="true" 
						value="#{MbFraudData.activeFraudData.operAmount}"
						title="#{MbFraudData.activeFraudData.operAmount}"/>

				<h:outputLabel value="#{lblCommon.currency}: " title="#{lblCommon.currency}" />
				<div class="#{EntityIcons.objectsMapS['ENTT0046'][MbFraudData.activeFraudData.operCurrency]}"/>
				<h:inputText readonly="true"
						value="#{MbFraudData.activeFraudData.operCurrency} - #{CurrencyUtils.currencyMap[MbFraudData.activeFraudData.operCurrency]}"
						title="#{MbFraudData.activeFraudData.operCurrency} - #{CurrencyUtils.currencyMap[MbFraudData.activeFraudData.operCurrency]}"
						rendered="#{MbFraudData.activeFraudData.operCurrency != null}"/>
				<h:inputText readonly="true"
						value="" rendered="#{MbFraudData.activeFraudData.operCurrency == null}"/>

				<h:outputLabel value="#{lblFrp.case}: " title="#{lblFrp.case}" />
				<h:inputText readonly="true" 
						value="#{MbFraudData.activeFraudData.caseName}"
						title="#{MbFraudData.activeFraudData.caseName}"/>
			</h:panelGrid>
		</h:panelGroup>		
	</h:form>
</ui:composition>
</html>