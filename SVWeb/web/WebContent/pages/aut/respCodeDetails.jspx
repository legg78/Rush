<?xml version="1.0" encoding="UTF-8" ?>

<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:a4j="http://richfaces.org/a4j"
	xmlns:rich="http://richfaces.org/rich">
<ui:composition>
	<f:loadBundle var="lblPrd" basename="ru.bpc.sv2.ui.bundles.Prd" />
	<h:form id="respCodeDetailsForm">
		<h:panelGroup id="respCodeDetailsWrapper" layout="block" styleClass="carddata-wrapper" 
				style="height: 100%; overflow-y: auto;overflow-x: hidden;position:relative;">
			<rich:dataTable id="respCodeDetailsTable"
					value="#{MbAutRespCode.emptyTable}" 
					var="field"
					styleClass="res-table noHeader" width="100%">
				<rich:columnGroup rowClasses=",odd" 
						columnClasses="firstCol alignLeft width20, width80 width250 noPadding">
					<rich:column styleClass="detailsCategory expandedBranch">
						<h:graphicImage url="/images/minus.gif" onclick="showHideDetails(this)"/>
						<h:outputLabel value="#{lblPrd.general_data}:" />
					</rich:column>
					<rich:column>
						<script>updateHeight();</script>
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblCommon.id}:" title="#{lblCommon.id}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbAutRespCode.activeItem.id}"
								title="#{MbAutRespCode.activeItem.id}"/>
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblAut.oper_type}:" title="#{lblAut.oper_type}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel rendered="#{MbAutRespCode.activeItem.operType != null }"
								value="#{DictUtils.articles[MbAutRespCode.activeItem.operType]}"
								title="#{DictUtils.articles[MbAutRespCode.activeItem.operType]}" />
						<h:outputLabel rendered="#{MbAutRespCode.activeItem.operType =='%' }" value="#{lblCommon.any}"/>
						<h:outputLabel value="" rendered="#{MbAutRespCode.activeItem.operType == null }" />
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblAut.msg_type}:" title="#{lblAut.msg_type}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel rendered="#{MbAutRespCode.activeItem.msgType != null }"
								value="#{DictUtils.articles[MbAutRespCode.activeItem.msgType]}"
								title="#{DictUtils.articles[MbAutRespCode.activeItem.msgType]}"/>
						<h:outputLabel rendered="#{MbAutRespCode.activeItem.msgType =='%' }" value="#{lblCommon.any}"/>								
						<h:outputLabel value="" rendered="#{MbAutRespCode.activeItem.msgType == null }" />
					</rich:column>
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblAut.is_reversal}:" title="#{lblAut.is_reversal}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{lblCommon.yes}" title="#{lblCommon.yes}"
								rendered="#{MbAutRespCode.activeItem.isReversal == 1}"/>
						<h:outputLabel value="#{lblCommon.no}" title="#{lblCommon.no}"
								rendered="#{MbAutRespCode.activeItem.isReversal == 0}"/>
						<h:outputLabel value="#{lblCommon.any}" title="#{lblCommon.any}"
								rendered="#{MbAutRespCode.activeItem !=null and MbAutRespCode.activeItem.isReversal == null}"
								/>
					</rich:column>
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblAut.response_code}:" title="#{lblAut.response_code}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel rendered="#{MbAutRespCode.activeItem.respCode != null}"
								value="#{DictUtils.articles[MbAutRespCode.activeItem.respCode]}"
								title="#{DictUtils.articles[MbAutRespCode.activeItem.respCode]}"/>						
						<h:outputLabel rendered="#{MbAutRespCode.activeItem.respCode =='%' }" value="#{lblCommon.any}"/>								
						<h:outputLabel value="" rendered="#{MbAutRespCode.activeItem.respCode == null }" />
					</rich:column>
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblAut.sign_completion}:" title="#{lblAut.sign_completion}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel rendered="#{MbAutRespCode.activeItem.signCompletion != null }"
								value="#{DictUtils.articles[MbAutRespCode.activeItem.signCompletion]}"
								title="#{DictUtils.articles[MbAutRespCode.activeItem.signCompletion]}"/>						
						<h:outputLabel rendered="#{MbAutRespCode.activeItem.signCompletion =='%' }" value="#{lblCommon.any}"/>								
						<h:outputLabel value="" rendered="#{MbAutRespCode.activeItem.signCompletion == null }" />
					</rich:column>
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblAut.priority}:" title="#{lblAut.priority}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbAutRespCode.activeItem.priority}"
								title="#{MbAutRespCode.activeItem.priority}"/>
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblAut.auth_status}:" title="#{lblAut.auth_status}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel rendered="#{MbAutRespCode.activeItem.authStatus != null }"
								value="#{DictUtils.articles[MbAutRespCode.activeItem.authStatus]}"
								title="#{DictUtils.articles[MbAutRespCode.activeItem.authStatus]}" />
						<h:outputLabel value="" rendered="#{MbAutRespCode.activeItem.authStatus == null }" />
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblAut.status_reason}:" title="#{lblAut.status_reason}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel rendered="#{MbAutRespCode.activeItem.statusReason != null }"
								value="#{DictUtils.articles[MbAutRespCode.activeItem.statusReason]}"
								title="#{DictUtils.articles[MbAutRespCode.activeItem.statusReason]}"/>
						<h:outputLabel value="" rendered="#{MbAutRespCode.activeItem.statusReason == null }" />
					</rich:column>
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblAut.proc_mode}:" title="#{lblAut.proc_mode}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel rendered="#{MbAutRespCode.activeItem.procMode != null }"
								value="#{DictUtils.articles[MbAutRespCode.activeItem.procMode]}"
								title="#{DictUtils.articles[MbAutRespCode.activeItem.procMode]}" />
						<h:outputLabel value="" rendered="#{MbAutRespCode.activeItem.procMode == null }" />
					</rich:column>
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblAut.proc_type}:" title="#{lblAut.proc_type}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel rendered="#{MbAutRespCode.activeItem.procType != null }"
								value="#{DictUtils.articles[MbAutRespCode.activeItem.procType]}"
								title="#{DictUtils.articles[MbAutRespCode.activeItem.procType]}"
								/>
						<h:outputLabel value="" rendered="#{MbAutRespCode.activeItem.procType == null }" />
					</rich:column>

					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblOpr.sttl_type}:" title="#{lblOpr.sttl_type}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel rendered="#{MbAutRespCode.activeItem.procType != null }"
								value="#{DictUtils.articles[MbAutRespCode.activeItem.sttlType]}"
								title="#{DictUtils.articles[MbAutRespCode.activeItem.sttlType]}"
								/>
						<h:outputLabel rendered="#{MbAutRespCode.activeItem.sttlType =='%' }" value="#{lblCommon.any}"/>
						<h:outputLabel value="" rendered="#{MbAutRespCode.activeItem.sttlType == null }" />
					</rich:column>
				</rich:columnGroup>
			</rich:dataTable>
			<script>setDetailsState("respCodeDetailsForm:respCodeDetailsTable");</script>
		</h:panelGroup>
	</h:form>
</ui:composition>
</html>
