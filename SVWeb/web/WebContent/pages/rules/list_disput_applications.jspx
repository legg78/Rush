                                    <h:outputText value="#{lblFrp.case_id}" title="#{lblFrp.case_id}"/>
                                </f:facet>
                                <h:outputText value="#{item.id}"/>
                            </rich:column>

                            <rich:column id="ownerColumn" label="#{lblMsg.CASE_OWNER}" title="#{lblMsg.CASE_OWNER}"
                                         sortBy="#{'case_owner'}" width="9%">
