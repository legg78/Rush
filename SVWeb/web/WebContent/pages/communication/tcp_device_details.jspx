<?xml version="1.0" encoding="UTF-8"?>
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core"
      xmlns:a4j="http://richfaces.org/a4j"
      xmlns:rich="http://richfaces.org/rich"
	  xmlns:bre="http://www.bpc.ru/jsf/richfaces-ext">
<ui:composition>	
	<f:loadBundle var="lblCmn" basename="ru.bpc.sv2.ui.bundles.Cmn"/>
	<f:loadBundle var="lblPrd" basename="ru.bpc.sv2.ui.bundles.Prd"/>
	
	<h:form id="tcpDeviceDetailsForm">
		<h:panelGroup layout="block" styleClass="langSelector">
			<h:panelGrid columns="2" styleClass="cardholderdata oneColumnTable"
					id="detailsData" columnClasses=",secondCol">
				<h:outputLabel for="lang" value="#{lblCommon.lang_selector}: "
						title="#{lblCommon.lang_selector}" />
				<ui:param name="callScript" value="updateContextHeight('#{empty subviewId ? 'contextPanelView' : subviewId}')" />
				<h:selectOneMenu id="lang"
						disabled="#{(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.id == null}"
						value="#{(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.lang}"
						valueChangeListener="#{(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).changeLanguage}">
					<a4j:support event="onchange" reRender="tcpDeviceDetailsForm" limitToList="true" oncomplete="#{mode=='context' ? callScript : 'updateHeight()'};"/>
				    <f:selectItems value="#{(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).languages}"/>
				</h:selectOneMenu>
			</h:panelGrid>
		</h:panelGroup>
					
		<h:panelGroup id="tcpDeviceDetailsWrapper" layout="block" styleClass="carddata-wrapper" 
				style="height: 100%; overflow-y: auto;overflow-x: hidden;position:relative;">
			<rich:dataTable id="tcpDeviceDetailsTable"
					value="#{(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).emptyTable}" 
					var="field"
					styleClass="res-table noHeader" width="100%">
				<rich:columnGroup rowClasses=",odd" 
						columnClasses="firstCol alignLeft width20, width80 width250 noPadding">
					<rich:column styleClass="detailsCategory expandedBranch">
						<h:graphicImage url="/images/minus.gif" onclick="showHideDetails(this)"/>
						<h:outputLabel value="#{lblPrd.general_data}:" />
					</rich:column>
					<rich:column>
						<script>updateHeight();</script>
					</rich:column>					
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblCommon.id}: " title="#{lblCommon.id}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.id}"
								title="#{(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.id}"/>
					</rich:column>

					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblCommon.institution}: " title="#{lblCommon.institution}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.instName}"
								title="#{(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.instName}"/>
					</rich:column>

					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblCommon.name}: " title="#{lblCommon.name}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.caption}"
								title="#{(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.caption}"/>
					</rich:column>

					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblCmn.comm_plugin}: " title="#{lblCmn.comm_plugin}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{DictUtils.articlesByLang[(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.lang][(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.commPlugin]}"
								title="#{DictUtils.articlesByLang[(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.lang][(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.commPlugin]}"/>
					</rich:column>
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblCommon.description}: " title="#{lblCommon.description}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.description}"
								title="#{(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.description}"/>
					</rich:column>
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblCmn.remote_address}: " title="#{lblCmn.remote_address}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.remoteAddress}"
								title="#{(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.remoteAddress}"/>
					</rich:column>
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblCmn.standard}: " title="#{lblCmn.standard}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.standardName}"
								title="#{(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.standardName}"/>
					</rich:column>
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblCmn.local_port}: " title="#{lblCmn.local_port}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.localPort}"
								title="#{(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.localPort}"/>
					</rich:column>
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblCmn.initiator}: " title="#{lblCmn.initiator}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{DictUtils.articlesByLang[(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.lang][(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.initiator]}"
								title="#{DictUtils.articlesByLang[(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.lang][(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.initiator]}"/>
					</rich:column>
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblCmn.remote_port}: " title="#{lblCmn.remote_port}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.remotePort}"
								title="#{(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.remotePort}"/>
					</rich:column>
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblCmn.format}: " title="#{lblCmn.format}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{DictUtils.articlesByLang[(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.lang][(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.format]}"
								title="#{DictUtils.articlesByLang[(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.lang][(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.format]}"/>
					</rich:column>
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblCmn.keep_alive}: " title="#{lblCmn.keep_alive}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.keepAlive}"
								title="#{(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.keepAlive}"/>
					</rich:column>
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblCmn.enabled}: " title="#{lblCmn.enabled}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.enabled}"
								title="#{(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.enabled}"/>
					</rich:column>
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblCmn.monitor_connection}: " title="#{lblCmn.monitor_connection}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.monitorConnection}"
								title="#{(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.monitorConnection}"/>
					</rich:column>
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblCmn.status_ok}: " title="#{lblCmn.status_ok}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.statusOk}"
								title="#{(tcpDeviceBean==null ? MbTcpIpDevices : tcpDeviceBean).activeDevice.statusOk}"/>
					</rich:column>
					
				</rich:columnGroup>
			</rich:dataTable>
			<script>setDetailsState("tcpDeviceDetailsForm:tcpDeviceDetailsTable");</script>
		</h:panelGroup>
	</h:form>

	
</ui:composition>
</html>
