<?xml version="1.0" encoding="UTF-8"?>
<html	xmlns="http://www.w3.org/1999/xhtml"
		xmlns:f="http://java.sun.com/jsf/core"
		xmlns:h="http://java.sun.com/jsf/html"
		xmlns:ui="http://java.sun.com/jsf/facelets"
		xmlns:a4j="http://richfaces.org/a4j"
		xmlns:rich="http://richfaces.org/rich"
		xmlns:bm="http://www.bpc.ru/jsf/misc"
		xmlns:bre="http://www.bpc.ru/jsf/richfaces-ext">
<ui:composition>
	<f:loadBundle var="lblPrd" basename="ru.bpc.sv2.ui.bundles.Prd" />
	<h:form id="desKeyDetailsForm">
		<h:panelGroup id="desKeyDetailsWrapper" layout="block" styleClass="carddata-wrapper" 
				style="height: 100%; overflow-y: auto;overflow-x: hidden;position:relative;">
			<rich:dataTable id="desKeyDetailsTable"
					value="#{MbDesKeys.emptyTable}" 
					var="field"
					styleClass="res-table noHeader" width="100%">
				<rich:columnGroup rowClasses=",odd" 
						columnClasses="firstCol alignLeft width20, width80 width250 noPadding">
					<rich:column styleClass="detailsCategory expandedBranch">
						<h:graphicImage url="/images/minus.gif" onclick="showHideDetails(this)"/>
						<h:outputLabel value="#{lblPrd.general_data}:" />
					</rich:column>
					<rich:column>
						<script>updateHeight();</script>
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblSec.key_type}:" title="#{lblSec.key_type}" />
					</rich:column>
					<rich:column>
						<h:outputLabel rendered="#{MbDesKeys.activeDesKey.keyType != null}"
								value="#{DictUtils.articles[MbDesKeys.activeDesKey.keyType]}"
								title="#{DictUtils.articles[MbDesKeys.activeDesKey.keyType]}"/>
						<h:outputLabel value="" rendered="#{MbDesKeys.activeDesKey.keyType == null}"/>
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblSec.standard_key_type}:" title="#{lblSec.standard_key_type}" />
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{DictUtils.articles[MbDesKeys.activeDesKey.standardKeyType]}"
								title="#{DictUtils.articles[MbDesKeys.activeDesKey.standardKeyType]}"
								rendered="#{MbDesKeys.activeDesKey != null and MbDesKeys.activeDesKey.standardKeyType != null}"/>
						<h:outputLabel value="" rendered="#{MbDesKeys.activeDesKey == null or MbDesKeys.activeDesKey.standardKeyType == null}" />
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblHsm.lmk}:" title="#{lblHsm.lmk}" />
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbDesKeys.activeDesKey.lmkDescription}"
								title="#{MbDesKeys.activeDesKey.lmkDescription}"
								style="white-space: normal"/>
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblSec.key_prefix}:" title="#{lblSec.key_prefix}" />
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbDesKeys.activeDesKey.keyPrefix}" 
								title="#{MbDesKeys.activeDesKey.keyPrefix}"  />
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblSec.key_index}:" title="#{lblSec.key_index}" />
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbDesKeys.activeDesKey.keyIndex}"
								title="#{MbDesKeys.activeDesKey.keyIndex}" />
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblSec.key_length}:" title="#{lblSec.key_length}" />
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbDesKeys.activeDesKey.keyLength}"
								title="#{MbDesKeys.activeDesKey.keyLength}" />
					</rich:column>
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblSec.check_value}:" title="#{lblSec.check_value}" />
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbDesKeys.activeDesKey.checkValue}"
								title="#{MbDesKeys.activeDesKey.checkValue}"/>
					</rich:column>
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblSec.key}:" title="#{lblSec.key}" />
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbDesKeys.activeDesKey.keyValue}"
								title="#{MbDesKeys.activeDesKey.keyValue}" />
					</rich:column>
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblSec.object_description}:" title="#{lblSec.object_description}" />
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbDesKeys.activeDesKey.objectDescription}"
								title="#{MbDesKeys.activeDesKey.objectDescription}"
								style="white-space: normal"/>
					</rich:column>
				</rich:columnGroup>
			</rich:dataTable>
			<script>setDetailsState("desKeyDetailsForm:desKeyDetailsTable");</script>
		</h:panelGroup>
	</h:form>
</ui:composition>
</html>
