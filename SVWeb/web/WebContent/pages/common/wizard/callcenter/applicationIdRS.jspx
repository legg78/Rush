<?xml version="1.0" encoding="UTF-8"?>
<html xmlns="http://www.w3.org/1999/xhtml" xmlns:f="http://java.sun.com/jsf/core"
      xmlns:h="http://java.sun.com/jsf/html" xmlns:ui="http://java.sun.com/jsf/facelets">
<ui:composition template="#{MbApplicationIdRS.detailsSubPage}">
    <ui:define name="stepContent">
        <f:loadBundle var="lblCommon" basename="ru.bpc.sv2.ui.bundles.App" />
        <!--@elvariable id="stepBean" type="ru.bpc.sv2.ui.common.wizard.callcenter.MbApplicationIdRS"-->
        <h:form>
            <ui:include src="/pages/common/wizard/commonWizardActions.jspx">
                <ui:param name="bean" value="#{bean}" />
                <ui:param name="action" value="#{action}" />
                <ui:param name="viewId" value="#{viewId}" />
            </ui:include>
        </h:form>

        <h:panelGroup layout="block" styleClass="cardholderdata-wrapper">
            <h:panelGrid columns="2" styleClass="cardholderdata"
                         columnClasses="width-150, width250">

                <h:outputLabel for="applicationId" title="#{lblApp.application_id}"
                               value="#{lblApp.application_id}:" />
                <h:inputText id="applicationId" value="#{stepBean.appId}" label="#{stepBean.appId}"
                             readonly="true">
                </h:inputText>

            </h:panelGrid>
        </h:panelGroup>
    </ui:define>
</ui:composition>
</html>
