<?xml version="1.0" encoding="UTF-8"?>
<html	xmlns="http://www.w3.org/1999/xhtml"
		xmlns:f="http://java.sun.com/jsf/core"
		xmlns:h="http://java.sun.com/jsf/html"
		xmlns:ui="http://java.sun.com/jsf/facelets"
		xmlns:a4j="http://richfaces.org/a4j"
		xmlns:rich="http://richfaces.org/rich"
		xmlns:bm="http://www.bpc.ru/jsf/misc"
		xmlns:bre="http://www.bpc.ru/jsf/richfaces-ext">
<ui:composition>
<h:form id="subscriberDetailsForm">
	<h:panelGroup layout="block" styleClass="cardholderdata-wrapper">
		<h:panelGrid columns="2" styleClass="cardholderdata oneColumnTable" width="100%" columnClasses="firstCol,secondCol">        
			
			<h:outputLabel value="#{lblCommon.event_type}: " title="#{lblCommon.event_type}" />
			<h:inputText value="#{DictUtils.articles[MbEventSubscribersSearch.activeSubscriber.eventType]}" readonly="true"/>
			
			<h:outputLabel value="#{lblCommon.procedure_name}: " title="#{lblCommon.procedure_name}" />
			<h:inputText value="#{MbEventSubscribersSearch.activeSubscriber.procedureName}" readonly="true"/>
		
		
			<h:outputLabel value="#{lblCommon.institution}: " title="#{lblCommon.institution}" />
			<h:inputText value="#{MbEventSubscribersSearch.activeSubscriber.instName}" readonly="true"/>
			
		</h:panelGrid>		
	</h:panelGroup>
</h:form>   
</ui:composition>
</html>