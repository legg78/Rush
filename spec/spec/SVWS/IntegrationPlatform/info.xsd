<?xml version="1.0" encoding="UTF-8"?>
<!--suppress XmlDefaultAttributeValue -->
<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://bpc.ru/SVXP"
		xmlns:tns="http://bpc.ru/SVXP" elementFormDefault="qualified">

	<complexType name="balance">
		<sequence>
			<element name="balance_type" minOccurs="1" maxOccurs="1">
				<simpleType>
					<union memberTypes="tns:balance_type tns:dictionary_article" />
				</simpleType>
			</element>
			<element name="amount" type="decimal" maxOccurs="1" minOccurs="1"/>
			<element name="currency" type="tns:currency" maxOccurs="1" minOccurs="1" />
		</sequence>
	</complexType>

	<complexType name="accounts">
		<sequence>
			<element name="account" type="tns:account" maxOccurs="unbounded" nillable="true" minOccurs="0"/>
		</sequence>
	</complexType>

	<complexType name="account">
		<sequence>
			<element name="account_id" type="long" minOccurs="1" maxOccurs="1"/>
			<element name="account_number" minOccurs="1" maxOccurs="1" type="string"/>
			<element name="account_type" minOccurs="1" maxOccurs="1" type="string"/>
			<element name="account_type_name" minOccurs="1" maxOccurs="1" type="string"/>
			<element name="currency" minOccurs="1" maxOccurs="1" type="string"/>
			<element name="currency_name" minOccurs="1" maxOccurs="1" type="string"/>
			<element name="status" minOccurs="1" maxOccurs="1" type="string"/>
			<element name="status_name" minOccurs="1" maxOccurs="1" type="string"/>
			<element name="aval_balance" minOccurs="1" maxOccurs="1" type="decimal"/>
			<element name="hold_balance" minOccurs="1" maxOccurs="1" type="decimal"/>
			<element name="agent_id" minOccurs="1" maxOccurs="1" type="long"/>
			<element name="agent_name" minOccurs="1" maxOccurs="1" type="string"/>
			<element name="open_date" minOccurs="1" maxOccurs="1" type="dateTime"/>
			<element name="close_date" minOccurs="0" maxOccurs="1" type="dateTime"/>
			<element name="owner_name" minOccurs="1" maxOccurs="1" type="string"/>
			<element name="request_balance" minOccurs="0" maxOccurs="1" type="decimal"/>
		</sequence>
	</complexType>

	<complexType name="account_payment_details">
		<sequence>
			<element name="account_number" type="string" minOccurs="1" maxOccurs="1"/>
			<element name="recipient_name" type="string" minOccurs="1" maxOccurs="1"/>
			<element name="bank_name" type="string" minOccurs="1" maxOccurs="1"/>
			<element name="bic" type="string" minOccurs="0" maxOccurs="1"/>
			<element name="tin" type="string" minOccurs="0" maxOccurs="1"/>
			<element name="corr_account" type="string" minOccurs="0" maxOccurs="1"/>
			<element name="bank_address" type="string" minOccurs="0" maxOccurs="1"/>
		</sequence>
	</complexType>

	<simpleType name="balance_type">
		<restriction base="string">
			<minLength value="5"/>
			<maxLength value="8"/>
			<enumeration value="BLTP0001"><!-- Ledger --></enumeration>
			<enumeration value="BLTP0003"><!-- Fees --></enumeration>
		</restriction>
	</simpleType>

	<simpleType name="currency">
		<restriction base="string"><!-- 3 digit ISO currency code -->
			<minLength value="3"/>
			<maxLength value="3"/>
		</restriction>
	</simpleType>

	<simpleType name="dictionary_article">
		<restriction base="string">
			<minLength value="5"/>
			<maxLength value="8"/>
		</restriction>
	</simpleType>

	<simpleType name="auth_code">
		<restriction base="string">
			<minLength value="6" />
			<maxLength value="6" />
		</restriction>
	</simpleType>

	<element name="card" type="tns:card"/>
	<complexType name="card">
		<sequence>
			<!-- Mandatory fields -->
			<element name="id" type="string" minOccurs="1" maxOccurs="1"/>
			<element name="card_instance_id" type="long" minOccurs="1" maxOccurs="1"/>
			<element name="card_mask" type="string" minOccurs="1" maxOccurs="1"/>
			<element name="seq_number" type="string" minOccurs="1" maxOccurs="1"/>
			<element name="expir_date" type="string" minOccurs="1" maxOccurs="1"/>
			<element name="person_name" type="string" minOccurs="1" maxOccurs="1"/>
			<element name="category" type="string" minOccurs="1" maxOccurs="1"/>
			<element name="network_id" type="long" minOccurs="1" maxOccurs="1"/>
			<element name="network_name" type="string" minOccurs="1" maxOccurs="1"/>
			<!-- Optional fields -->
			<element name="card_number" type="string" minOccurs="0" maxOccurs="1"/>
			<element name="category_name" type="string" minOccurs="0" maxOccurs="1"/>
			<element name="card_type" type="string" minOccurs="0" maxOccurs="1"/>
			<element name="status" type="string" minOccurs="0" maxOccurs="1"/>
			<element name="status_name" type="string" minOccurs="0" maxOccurs="1"/>
			<element name="state" type="string" minOccurs="0" maxOccurs="1" />
			<element name="state_name" type="string" minOccurs="0" maxOccurs="1"/>
			<element name="delivery_status" type="string" minOccurs="0" maxOccurs="1" />
			<element name="delivery_status_name" type="string" minOccurs="0" maxOccurs="1"/>
			<element name="bin_currency" type="string" minOccurs="0" maxOccurs="1"/>
			<element name="currency_name" type="string" maxOccurs="1" minOccurs="1"/>
			<element name="aval_balance" type="decimal" minOccurs="0" maxOccurs="1"/>
			<element name="hold_balance" type="decimal" minOccurs="0" maxOccurs="1"/>
			<element name="customer_id" type="long" minOccurs="0" maxOccurs="1"/>
			<element name="customer_number" type="string" minOccurs="0" maxOccurs="1"/>
			<element name="inst_id" type="tns:institution_id" minOccurs="1" maxOccurs="1"/>
			<element name="account_number" type="string" minOccurs="0" maxOccurs="1"/>
			<element name="account_id" type="long" minOccurs="0" maxOccurs="1"/>
			<element name="product_id" type="long" minOccurs="0" maxOccurs="1"/>
			<element name="product_name" type="string" minOccurs="0" maxOccurs="1"/>
			<element name="creation_date" type="dateTime" minOccurs="0" maxOccurs="1"/>
			<element name="embossed_name" type="string" minOccurs="0" maxOccurs="1"/>
			<element name="cardholder_name" type="tns:cardholder_name" minOccurs="0" maxOccurs="1"/>
		</sequence>
	</complexType>

	<element name="cards" type="tns:cards"/>
	<complexType name="cards">
		<sequence>
			<element name="card" type="tns:card" maxOccurs="unbounded" minOccurs="0"/>
		</sequence>
	</complexType>

	<element name="customer_ntf_settings" type="tns:customer_ntf_settings"/>

	<element name="customer_ntfs_settings" type="tns:customer_ntfs_settings"/>

	<complexType name="customer_ntfs_settings">
		<sequence>
			<element name="customer_ntf_settings" type="tns:customer_ntf_settings" maxOccurs="unbounded" minOccurs="0"/>
		</sequence>
	</complexType>

	<complexType name="customer_ntf_settings">
		<sequence>
			<element name="entity_type" type="string" minOccurs="1" maxOccurs="1"/>
			<element name="entity_type_name" type="string" minOccurs="1" maxOccurs="1"/>
			<element name="object_id" type="long" minOccurs="0" maxOccurs="1"/>
			<element name="event_type" type="string" minOccurs="1" maxOccurs="1"/>
			<element name="event_type_name" type="string" minOccurs="1" maxOccurs="1"/>
			<element name="channel_id" type="int" minOccurs="1" maxOccurs="1"/>
			<element name="channel_name" type="string" minOccurs="1" maxOccurs="1"/>
			<element name="delivery_address" type="string" minOccurs="1" maxOccurs="1"/>
		</sequence>
	</complexType>

	<complexType name="service_term">
		<sequence>
			<element name="name" type="string" maxOccurs="1" minOccurs="1"/>
			<element name="type" type="string" maxOccurs="1" minOccurs="0"/>
			<element name="simple" type="tns:simple_service_term" maxOccurs="1" minOccurs="0"/>
			<element name="limit" type="tns:limit" maxOccurs="1" nillable="true" minOccurs="0"/>
			<element name="fee" type="tns:fee" maxOccurs="1" nillable="true" minOccurs="0"/>
			<element name="cycle" type="string" maxOccurs="1" nillable="true" minOccurs="0"/>
		</sequence>
	</complexType>

	<complexType name="flexible_parameter">
		<sequence>
			<element name="name" type="string" maxOccurs="1" minOccurs="1"/>
			<element name="data_type" type="tns:data_type" maxOccurs="1" minOccurs="1"/>
			<element name="value_char" type="string" maxOccurs="1" nillable="true" minOccurs="0"/>
			<element name="value_number" type="decimal" maxOccurs="1" nillable="true" minOccurs="0"/>
			<element name="value_date" type="dateTime" maxOccurs="1" nillable="true" minOccurs="0"/>
		</sequence>
	</complexType>

	<complexType name="limit">
		<sequence>
			<element name="type" type="string" maxOccurs="1" minOccurs="1"/>
			<element name="currency" type="string" maxOccurs="1" nillable="true" minOccurs="0"/>
			<element name="sum_value" type="decimal" maxOccurs="1" nillable="true" minOccurs="0"/>
			<element name="sum_limit" type="decimal" maxOccurs="1" nillable="true" minOccurs="0"/>
		</sequence>
	</complexType>


	<simpleType name="data_type">
		<restriction base="tns:dictionary_article">
			<enumeration value="DTTPCHAR"/>
			<enumeration value="DTTPNMBR"/>
			<enumeration value="DTTPDATE"/>
		</restriction>
	</simpleType>

	<complexType name="simple_service_term">
		<sequence>
			<element name="data_type" type="tns:data_type" maxOccurs="1" minOccurs="1"/>
			<element name="value_char" type="string" maxOccurs="1" nillable="true" minOccurs="0"/>
			<element name="value_number" type="decimal" maxOccurs="1" nillable="true" minOccurs="0"/>
			<element name="value_date" type="dateTime" maxOccurs="1" nillable="true" minOccurs="0"/>
		</sequence>
	</complexType>

	<complexType name="fee">
		<sequence>
			<element name="type" type="string" maxOccurs="1" minOccurs="1"/>
			<element name="currency" type="string" maxOccurs="1" minOccurs="1"/>
			<element name="value" type="decimal" maxOccurs="1" nillable="true" minOccurs="0"/>
		</sequence>
	</complexType>

	<element name="statuses" type="tns:statuses"/>

	<complexType name="statuses">
		<sequence>
			<element name="status" type="string"
					 maxOccurs="unbounded" nillable="true" minOccurs="0"/>
		</sequence>
	</complexType>

	<element name="operation" type="tns:operation"/>

	<element name="operations" type="tns:operations"/>

	<complexType name="operation">
		<sequence>
			<element name="id" minOccurs="1" maxOccurs="1">
				<simpleType>
					<restriction base="long">
						<minInclusive value="1"/>
						<maxInclusive value="9999999999999999"/>
					</restriction>
				</simpleType>
			</element>
			<element name="original_id" type="string" maxOccurs="1" nillable="true" minOccurs="0"/>
			<element name="card_mask" type="string" minOccurs="1" maxOccurs="1"/>
			<element name="account_number" type="string" minOccurs="1" maxOccurs="1"/>
			<element name="card_seq_number" type="string" minOccurs="0" maxOccurs="1"/>
			<element name="is_reversal" type="boolean" minOccurs="0" maxOccurs="1"/>
			<element name="oper_date" type="dateTime" minOccurs="0" maxOccurs="1"/>
			<element name="oper_type" type="string" minOccurs="0" maxOccurs="1"/>
			<element name="oper_type_name" type="string" minOccurs="0" maxOccurs="1"/>
			<element name="status" type="string" minOccurs="0" maxOccurs="1"/>
			<element name="status_name" type="string" minOccurs="0" maxOccurs="1"/>
			<element name="oper_amount" type="string" minOccurs="0" maxOccurs="1"/>
			<element name="oper_currency" type="string" maxOccurs="1" minOccurs="0"/>
			<element name="currency_name" type="string" maxOccurs="1" minOccurs="0"/>
			<element name="mcc" type="string" minOccurs="0" maxOccurs="1"/>
			<element name="merchant_number" type="string" minOccurs="0" maxOccurs="1"/>
			<element name="merchant_name" type="string" minOccurs="0" maxOccurs="1"/>
			<element name="merchant_address" type="string" minOccurs="0" maxOccurs="1"/>
			<element name="balance" type="string" minOccurs="0" maxOccurs="1"/>
			<element name="currency" type="string" minOccurs="0" maxOccurs="1"/>
			<element name="account_amount" type="string" minOccurs="0" maxOccurs="1"/>
			<element name="account_currency" type="string" minOccurs="0" maxOccurs="1"/>
			<element name="host_date" type="dateTime" minOccurs="0" maxOccurs="1"/>
			<element name="originator_refnum" type="string" maxOccurs="1" minOccurs="0"/>
			<element name="external_auth_id" type="string" maxOccurs="1" minOccurs="0"/>
			<element name="resp_code" type="tns:dictionary_article" maxOccurs="1" minOccurs="0"/>
			<element name="is_advice" maxOccurs="1" minOccurs="0">
				<simpleType>
					<restriction base="int">
						<minInclusive value="0"/>
						<maxInclusive value="1"/>
					</restriction>
				</simpleType>
			</element>
			<element name="cat_level" type="tns:dictionary_article" maxOccurs="1" minOccurs="0"/>
			<element name="card_data_input_cap" type="tns:dictionary_article" maxOccurs="1" minOccurs="0"/>
			<element name="crdh_auth_cap" type="tns:dictionary_article" maxOccurs="1" minOccurs="0"/>
			<element name="card_capture_cap" type="tns:dictionary_article" maxOccurs="1" minOccurs="0"/>
			<element name="terminal_operating_env" type="tns:dictionary_article" maxOccurs="1" minOccurs="0"/>
			<element name="crdh_presence" type="tns:dictionary_article" maxOccurs="1" minOccurs="0"/>
			<element name="card_presence" type="tns:dictionary_article" maxOccurs="1" minOccurs="0"/>
			<element name="card_data_input_mode" type="tns:dictionary_article" maxOccurs="1" minOccurs="0"/>
			<element name="crdh_auth_method" type="tns:dictionary_article" maxOccurs="1" minOccurs="0"/>
			<element name="crdh_auth_entity" type="tns:dictionary_article" maxOccurs="1" minOccurs="0"/>
			<element name="card_data_output_cap" type="tns:dictionary_article" maxOccurs="1" minOccurs="0"/>
			<element name="terminal_output_cap" type="tns:dictionary_article" maxOccurs="1" minOccurs="0"/>
			<element name="pin_capture_cap" type="tns:dictionary_article" maxOccurs="1" minOccurs="0"/>
			<element name="pin_presence" type="tns:dictionary_article" maxOccurs="1" minOccurs="0"/>
			<element name="terminal_number" type="tns:dictionary_article" maxOccurs="1" minOccurs="0"/>
			<element name="auth_code" type="tns:auth_code" maxOccurs="1" minOccurs="0"/>
		</sequence>
	</complexType>

	<complexType name="participant">
		<sequence>

			<element name="client_id_type" nillable="true" minOccurs="0" maxOccurs="1">
				<simpleType>
					<restriction base="tns:dictionary_article">
						<enumeration value="CITPACCT" /><!-- Identification by account -->
						<enumeration value="CITPCARD" /><!-- Identification by card number -->
						<enumeration value="CITPCNTR" /><!-- Identification by contract number -->
						<enumeration value="CITPCUST" /><!-- Identification by customer number -->
						<enumeration value="CITPEMAI" /><!-- Identification by e-mail -->
						<enumeration value="CITPMBPH" /><!-- Identification by mobile phone number -->
						<enumeration value="CITPNONE" /><!-- No identification -->
						<enumeration value="CITPUNKN" /><!-- Unknown identification -->
						<enumeration value="CITPXCRD" /><!-- Identification by linked card -->
					</restriction>
				</simpleType>
			</element>
			<element name="client_id_value" nillable="true" minOccurs="0" maxOccurs="1">
				<simpleType>
					<restriction base="string">
						<minLength value="2" />
						<maxLength value="200" />
					</restriction>
				</simpleType>
			</element>
			<element name="card_number" nillable="true" minOccurs="0" maxOccurs="1">
				<simpleType>
					<restriction base="string">
						<minLength value="16" />
						<maxLength value="19" />
					</restriction>
				</simpleType>
			</element>
			<element name="card_seq_number" nillable="true" minOccurs="0" maxOccurs="1">
				<simpleType>
					<restriction base="int">
						<minInclusive value="1" />
						<maxInclusive value="9999" />
					</restriction>
				</simpleType>
			</element>
			<element name="card_expir_date" type="date" nillable="true" minOccurs="0" maxOccurs="1" />
			<element name="inst_id" type="tns:institution_id" nillable="true" minOccurs="0" maxOccurs="1"/>
			<element name="network_id" nillable="true" minOccurs="0" maxOccurs="1">
				<simpleType>
					<restriction base="int">
						<minInclusive value="1000" />
						<maxInclusive value="9999" />
					</restriction>
				</simpleType>
			</element>
			<element name="auth_code" type="tns:auth_code" nillable="true" minOccurs="0" maxOccurs="1"/>
			<element name="account_number" type="string" nillable="true" minOccurs="0" maxOccurs="1"/>
			<element name="account_type" type="tns:dictionary_article" nillable="true" minOccurs="0" maxOccurs="1"/>
			<element name="account_currency" type="tns:currency" nillable="true" minOccurs="0" maxOccurs="1"/>
			<element name="account_amount" type="decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<element name="aval_bal_before" type="decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
			<element name="aval_bal_after" type="decimal" nillable="true" minOccurs="0" maxOccurs="1"/>
		</sequence>
	</complexType>

	<complexType name="payment_order">
		<sequence>

			<element name="payment_order_id" minOccurs="1" maxOccurs="1">
				<simpleType>
					<restriction base="long">
						<minInclusive value="1"/>
						<maxInclusive value="9999999999999999"/>
					</restriction>
				</simpleType>
			</element>

			<element name="payment_order_status" type="tns:dictionary_article" nillable="true" minOccurs="0" maxOccurs="1"/>

			<element name="payment_parameter" type="tns:payment_parameter" nillable="true" minOccurs="0" maxOccurs="unbounded"/>

			<element name="document" type="tns:document" nillable="true" minOccurs="0" maxOccurs="1"/>

		</sequence>
	</complexType>

	<complexType name="payment_parameter">
		<sequence>
			<element name="payment_parameter_name" minOccurs="1" maxOccurs="1">
				<simpleType>
					<restriction base="string">
						<minLength value="1"/>
						<maxLength value="200"/>
					</restriction>
				</simpleType>
			</element>

			<element name="payment_parameter_value" minOccurs="1" maxOccurs="1">
				<simpleType>
					<restriction base="string">
						<minLength value="1"/>
						<maxLength value="2000"/>
					</restriction>
				</simpleType>
			</element>

		</sequence>
	</complexType>


	<complexType name="amount">
		<sequence>
			<element name="amount_value" type="decimal" minOccurs="1" maxOccurs="1"/>
			<element name="currency" type="tns:currency" minOccurs="1" maxOccurs="1"/>
		</sequence>
	</complexType>

	<complexType name="document">
		<sequence>

			<element name="document_id" minOccurs="1" maxOccurs="1">
				<simpleType>
					<restriction base="long">
						<minInclusive value="1"/>
						<maxInclusive value="9999999999999999"/>
					</restriction>
				</simpleType>
			</element>

			<element name="document_type" type="tns:dictionary_article" minOccurs="1" maxOccurs="1">
			</element>

			<element name="document_date" type="date" minOccurs="1" maxOccurs="1"/>

			<element name="document_number" minOccurs="1" maxOccurs="1">
				<simpleType>
					<restriction base="string">
						<minLength value="2"/>
						<maxLength value="200"/>
					</restriction>
				</simpleType>
			</element>

			<element name="document_content" type="tns:document_content" nillable="true" minOccurs="0" maxOccurs="unbounded"/>

		</sequence>
	</complexType>

	<complexType name="document_content">
		<sequence>

			<element name="content_type" minOccurs="1" maxOccurs="1">
				<simpleType>
					<restriction base="tns:dictionary_article">
						<enumeration value="DCCT0001"/> <!-- Main document -->
						<enumeration value="DCCT0002"/> <!-- Digital signature of client -->
						<enumeration value="DCCT0003"/> <!-- Registration mark -->
						<enumeration value="DCCT0004"/> <!-- Registration mark operator's digital signature -->
						<enumeration value="DCCT0005"/> <!-- Execution mark -->
						<enumeration value="DCCT0006"/> <!-- Execution mark operator's digital signature -->
					</restriction>
				</simpleType>
			</element>

			<element name="content" type="string" minOccurs="1" maxOccurs="1"/>

		</sequence>
	</complexType>

	<complexType name="operations">
		<sequence>
			<element name="operation" type="tns:operation" maxOccurs="unbounded" nillable="true" minOccurs="0"/>
		</sequence>
	</complexType>

	<complexType name="service_terms">
		<sequence>
			<element name="service_term" type="tns:service_term"
					 maxOccurs="unbounded" nillable="true" minOccurs="0">
			</element>
		</sequence>
	</complexType>

	<complexType name="flexible_parameters">
		<sequence>
			<element name="flexible_parameter"
					 type="tns:flexible_parameter" maxOccurs="unbounded" nillable="true" minOccurs="0">
			</element>
		</sequence>
	</complexType>

	<element name="invoice" type="tns:invoice"/>

	<complexType name="invoice">
		<sequence>
			<element name="account_number" type="string"/>
			<element name="currency" type="tns:currency"/>
			<element name="serial_number" type="int"/>
			<element name="invoice_type" type="tns:dictionary_article"/>
			<element name="exceed_limit" type="decimal"/>
			<element name="total_amount_due" type="decimal"/>
			<element name="own_funds" type="decimal"/>
			<element name="min_amount_due" type="decimal"/>
			<element name="start_date" type="date"/>
			<element name="invoice_date" type="date"/>
			<element name="grace_date" type="date"/>
			<element name="due_date" type="date"/>
			<element name="penalty_date" type="date"/>
			<element name="aging_period" type="int"/>
			<element name="is_tad_paid" type="int"/>
			<element name="is_mad_paid" type="int"/>
			<element name="statement_date" type="date"/>
			<element name="customer_name" type="string"/>
			<element name="customer_address" type="string"/>
			<element name="invoice_begin_date" type="date"/>
			<element name="invoice_end_date" type="date"/>
			<element name="total_income" type="decimal"/>
			<element name="total_expenses" type="decimal"/>
			<element name="interest_amount" type="decimal"/>
