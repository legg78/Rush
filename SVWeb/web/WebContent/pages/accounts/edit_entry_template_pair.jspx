											<a4j:support event="onchange" reRender="debitsTable, editCredit"/>
										</h:selectBooleanCheckbox>
										<h:outputText value="#{lblAcc.debit}" style="padding: 4px"/>
									</h:panelGroup>
									<h:panelGroup>
										<h:selectBooleanCheckbox
												value="#{MbEntryTemplates.newEntryTemplatePair.editCredit}"
												id="editCredit"
												disabled="#{MbEntryTemplates.newEntryTemplatePair.editCredit and not MbEntryTemplates.newEntryTemplatePair.editDebit}"
												valueChangeListener="#{MbEntryTemplates.changeCredit}">
											<a4j:support event="onchange" reRender="creditsTable, editDebit"/>
										</h:selectBooleanCheckbox>
										<h:outputText value="#{lblAcc.credit}" style="padding: 4px"/>
									</h:panelGroup>
								</h:panelGrid>
							</f:facet>

							<h:panelGrid columns="2" id="debitsTable" styleClass="cardholderdata" columnClasses="firstColModal, width250" >
