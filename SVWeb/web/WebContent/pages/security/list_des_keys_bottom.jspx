<?xml version="1.0" encoding="UTF-8"?>
<html xmlns="http://www.w3.org/1999/xhtml"
	  xmlns:f="http://java.sun.com/jsf/core"
	  xmlns:h="http://java.sun.com/jsf/html"
	  xmlns:ui="http://java.sun.com/jsf/facelets"
	  xmlns:a4j="http://richfaces.org/a4j"
	  xmlns:rich="http://richfaces.org/rich"
	  xmlns:bm="http://www.bpc.ru/jsf/misc"
	  xmlns:bre="http://www.bpc.ru/jsf/richfaces-ext">
<ui:composition>
	<f:loadBundle var="lblSec" basename="ru.bpc.sv2.ui.bundles.Sec" />
	<f:loadBundle var="lblHsm" basename="ru.bpc.sv2.ui.bundles.Hsm" />

	<h:form id="desKeysBtmForm" style="overflow:hidden;">
		<h:panelGroup id="desKeysTableWrapper" layout="block"
					  style="padding:0px;margin:0px;">
			<rich:extendedDataTable id="desKeysTable" styleClass="res-table"
									headerClass="res-table-header table-header-ext" rowClasses=",odd"
									selectedClass="res-table-selected" var="item"
									value="#{(desKeyBean==null ? MbDesKeysBottom : desKeyBean).desKeys}" selection="#{(desKeyBean==null ? MbDesKeysBottom : desKeyBean).itemSelection}"
									height="220px" enableContextMenu="true"
									tableState="#{(desKeyBean==null ? MbDesKeysBottom : desKeyBean).tableState}"
									noDataLabel="#{lblCommon['no_records_found']}">

				<a4j:support event="onselectionchange"
							 reRender="desKeysBtmForm:keysBtnPanel" />

				<rich:column id="key_type" label="#{lblSec.key_type}" title="Key type" sortBy="#{'keyType'}" width="20%">
					<f:facet name="header">
						<a4j:outputPanel layout="none">
							<script>
								updateHeight();
							</script>
							<h:outputText value="#{lblSec.key_type}"
										  title="#{lblSec.key_type}" />
						</a4j:outputPanel>
					</f:facet>
					<h:outputText value="#{DictUtils.articles[item.standardKeyType]}"
								  title="#{DictUtils.articles[item.standardKeyType]}"
								  rendered="#{item.standardKeyType != null}" />
					<h:outputText value="#{DictUtils.articles[item.keyType]}"
								  title="#{DictUtils.articles[item.keyType]}"
								  rendered="#{item.standardKeyType == null and item.keyType != null}" />
					<h:outputText value="" title=""
								  rendered="#{item.standardKeyType == null and item.keyType == null}" />
				</rich:column>

				<rich:column id="key_prefix" label="#{lblSec.key_prefix}" title="Key prefix" sortBy="#{'keyPrefix'}" width="10%">
					<f:facet name="header">
						<h:outputText value="#{lblSec.key_prefix}"
									  title="#{lblSec.key_prefix}" />
					</f:facet>
					<h:outputText value="#{item.keyPrefix}" title="#{item.keyPrefix}" />
				</rich:column>

				<rich:column id="key_index" label="#{lblSec.key_index}" title="Key index" sortBy="#{'keyIndex'}" width="10%">
					<f:facet name="header">
						<h:outputText value="#{lblSec.key_index}"
									  title="#{lblSec.key_index}" />
					</f:facet>
					<h:outputText value="#{item.keyIndex}" />
				</rich:column>

				<rich:column id="key_length" label="#{lblSec.key_length}" title="Key length" sortBy="#{'keyLength'}" width="10%">
					<f:facet name="header">
						<h:outputText value="#{lblSec.key_length}"
									  title="#{lblSec.key_length}" />
					</f:facet>
					<h:outputText value="#{item.keyLength}" />
				</rich:column>

				<rich:column id="check_value" label="#{lblSec.check_value}" title="Check value" sortBy="#{'checkValue'}"
							 width="10%">
					<f:facet name="header">
						<h:outputText value="#{lblSec.check_value}"
									  title="#{lblSec.check_value}" />
					</f:facet>
					<h:outputText value="#{item.checkValue}" title="#{item.checkValue}" />
				</rich:column>

				<rich:column id="key" label="#{lblSec.key}" title="Key" sortBy="#{'key'}" width="23%">
					<f:facet name="header">
						<h:outputText value="#{lblSec.key}" title="#{lblSec.key}" />
					</f:facet>
					<h:outputText value="#{item.keyValue}" title="#{item.keyValue}" />
				</rich:column>

				<rich:column id="lmk" label="#{lblHsm.lmk}" title="LMK" sortBy="#{'lmkDescription'}" width="15%" rendered="#{empty useHsm ? true : useHsm}">
					<f:facet name="header">
						<h:outputText value="#{lblHsm.lmk}" title="#{lblHsm.lmk}" />
					</f:facet>
					<h:outputText value="#{item.lmkDescription}"
								  title="#{item.lmkDescription}" />
				</rich:column>
			</rich:extendedDataTable>
		</h:panelGroup>
		<h:panelGroup layout="block"
					  styleClass="bottom_search_result_block_buttons">
			<h:panelGroup layout="block"
						  styleClass="bottom_search_result_left_buttons" rendered="#{desKeyBean==null}">

				<h:panelGrid id="keysBtnPanel" styleClass="fw buttons_block" columns="2" columnClasses=", table_tools_state">
					<h:panelGroup>
						<bre:taggedCommandButton id="viewBtn"
												 reRender="sec_des_key_view_modal_div" value="#{lblForm.view}"
												 action="#{MbDesKeysBottom.view}"
												 oncomplete="#{rich:component('sec_des_key_view_modal_panel')}.show()"
												 disabled="#{MbDesKeysBottom.activeDesKey == null}" />
						<bre:taggedCommandButton id="addBtn"
												 rendered="#{usession.inRole['ADD_DES_KEY']}"
												 reRender="sec_des_key_modal_header, sec_des_key_modal_div"
												 value="#{lblForm.add}"
												 oncomplete="#{rich:component('sec_des_key_modal_panel')}.show()"
												 action="#{MbDesKeysBottom.add}"
												 disabled="#{MbDesKeysBottom.filter.objectId == null}"
												 image="/images/create_doc.gif" />
						<bre:taggedCommandButton id="generateBtn"
												 rendered="#{usession.inRole['GENERATE_DES_KEY'] and (empty useHsm ? true : useHsm)}"
												 reRender="sec_des_key_generate_modal_div"
												 value="#{lblSec.generate}" action="#{MbDesKeysBottom.generate}"
												 oncomplete="#{rich:component('sec_des_key_generate_modal_panel')}.show()"
												 disabled="#{MbDesKeysBottom.filter.objectId == null}"
												 image="/images/edit.gif" />
						<bre:taggedCommandButton id="translateBtn"
												 reRender="sec_des_key_translate_modal_div"
												 value="#{lblSec.translate}" action="#{MbDesKeysBottom.translate}"
												 oncomplete="#{rich:component('sec_des_key_translate_modal_panel')}.show()"
												 disabled="#{MbDesKeysBottom.filter.objectId == null}"
												 rendered="#{usession.inRole['TRANSLATE_DES_KEY'] and MbDesKeysBottom.showTranslate and (empty useHsm ? true : useHsm)}"
												 image="/images/edit.gif" />

						<bre:taggedCommandButton id="deleteBtn"
												 value="#{lblForm.delete}"
												 disabled="#{MbDesKeysBottom.activeDesKey == null}"
												 rendered="#{usession.inRole['REMOVE_DES_KEY']}"
												 image="/images/delete.gif"
												 oncomplete="#{rich:component('deleteDesKeyView:confirmPanel')}.show()"/>
					</h:panelGroup>
					<h:panelGroup>
						<h:graphicImage url="/images/green-tick.png" title="#{lblCommon.save_table_state}" styleClass="action_icon"
										onclick="saveDesKeyTableState();"/>
						<h:graphicImage url="/images/red-cross.png" title="#{lblCommon.delete_table_state}" styleClass="action_icon"
										onclick="deleteDesKeyTableState();"/>
						<a4j:queue name="tablestatequeue"/>
						<a4j:jsFunction name="saveDesKeyTableState" action="#{MbDesKeysBottom.saveTableStateDB}" eventsQueue="tablestatequeue"/>
						<a4j:jsFunction name="deleteDesKeyTableState" action="#{MbDesKeysBottom.deleteTableStateDB}" eventsQueue="tablestatequeue"/>
					</h:panelGroup>
				</h:panelGrid>
			</h:panelGroup>
		</h:panelGroup>
	</h:form>

	<ui:include src="/pages/security/des_key_edit.jspx">
		<ui:param name="rerenderList" value="desKeysBtmForm" />
	</ui:include>

	<ui:include src="/pages/security/des_key_translate.jspx">
		<ui:param name="rerenderList" value="desKeysBtmForm" />
	</ui:include>

	<ui:include src="/pages/security/des_key_generate.jspx">
		<ui:param name="rerenderList" value="desKeysBtmForm" />
	</ui:include>

	<ui:include src="/pages/security/des_key_view.jspx"></ui:include>

	<ui:include src="/pages/utils/confirmPanel.jspx">
		<ui:param name="backingBean" value="#{MbDesKeysBottom}"/>
		<ui:param name="yesAction" value="delete"/>
		<ui:param name="warningMsg" value="#{lblMsg.confirm_delete}"/>
		<ui:param name="rerenderList" value="desKeysBtmForm"/>
		<ui:param name="confirmQuestion" value=" "/>
		<ui:param name="subviewId" value="deleteDesKeyView"/>
		<ui:param name="yes" value="#{lblForm.ok}"/>
		<ui:param name="no" value="#{lblForm.cancel}"/>
	</ui:include>


</ui:composition>
</html>
