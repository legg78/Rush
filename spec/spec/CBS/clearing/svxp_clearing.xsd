<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
        targetNamespace="http://bpc.ru/sv/SVXP/clearing"
        xmlns:tns="http://bpc.ru/sv/SVXP/clearing"
        elementFormDefault="qualified">

    <simpleType name="account_number">
        <restriction base="string">
            <minLength value="1"/>
            <maxLength value="32"/>
        </restriction>
    </simpleType>
    <simpleType name="amount_value">
        <restriction base="long">
            <minInclusive value="0"/>
            <maxInclusive value="9999999999999999"/>
        </restriction>
    </simpleType>
    <simpleType name="sign_amount_value">
        <restriction base="long">
            <minInclusive value="-9999999999999999"/>
            <maxInclusive value="9999999999999999"/>
        </restriction>
    </simpleType>
    <simpleType name="currency">
        <restriction base="string">
            <minLength value="3"/>
            <maxLength value="3"/>
        </restriction>
    </simpleType>
    <simpleType name="dict">
        <restriction base="string">
            <minLength value="5"/>
            <maxLength value="8"/>
        </restriction>
    </simpleType>

    <simpleType name="base64Hex">
        <restriction base="string">
            <pattern value="[A-F0-9]{0,}"/>
        </restriction>
    </simpleType>

    <simpleType name="string_200_type">
        <restriction base="string">
            <maxLength value="200"/>
        </restriction>
    </simpleType>

    <simpleType name="intId">
        <restriction base="int">
            <minInclusive value="0"/>
            <maxInclusive value="99999999"/>
        </restriction>
    </simpleType>

    <simpleType name="stringName">
        <restriction base="string">
           <minLength value="0"/>
           <maxLength value="4000"/>
        </restriction>
    </simpleType>

    <simpleType name="stringValue">
        <restriction base="string">
           <minLength value="0"/>
           <maxLength value="2000"/>
        </restriction>
    </simpleType>

    <simpleType name="tagValue">
        <restriction base="string">
            <minLength value="0"/>
            <maxLength value="992"/>
        </restriction>
    </simpleType>


    <complexType name="clearing">
        <sequence>
            <element name="file_id" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="long">
                        <maxInclusive value="9999999999999999"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="file_type" minOccurs="1" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="8"/>
                        <maxLength value="8"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="original_file_id" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="long">
                        <minInclusive value="1"/>
                        <maxInclusive value="9999999999999999"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="start_date" type="date" minOccurs="0" maxOccurs="1"/>

            <element name="end_date" type="date" minOccurs="0" maxOccurs="1"/>

            <element name="inst_id" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="int">
                        <minInclusive value="1"/>
                        <maxInclusive value="9999"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="operation" type="tns:operation" minOccurs="1" maxOccurs="unbounded"/>
        </sequence>
    </complexType>

    <complexType name="operation">
        <sequence>

            <element name="oper_id" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="long">
                        <minInclusive value="1"/>
                        <maxInclusive value="9999999999999999"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="oper_type" minOccurs="1" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="8"/>
                        <maxLength value="8"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="msg_type" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="8"/>
                        <maxLength value="8"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="sttl_type" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <pattern value="STTT[0-9A-Z_-]{1,4}"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="original_id" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="long">
                        <minInclusive value="1"/>
                        <maxInclusive value="9999999999999999"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="reconciliation_type" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="8"/>
                        <maxLength value="8"/>
                        <enumeration value="RCLMFULL"/>
                        <enumeration value="RCLMNONE"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="oper_date" type="dateTime" minOccurs="0"
                maxOccurs="1">
            </element>

            <element name="host_date" type="dateTime" minOccurs="0"
                maxOccurs="1">
            </element>

            <element name="oper_count" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="long">
                        <minInclusive value="0"/>
                        <maxInclusive value="9999999999999999"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="oper_amount" type="tns:amount" minOccurs="0"
                maxOccurs="1">
            </element>

            <element name="oper_request_amount" type="tns:amount"
                minOccurs="0" maxOccurs="1">
            </element>

            <element name="oper_surcharge_amount" type="tns:amount"
                minOccurs="0" maxOccurs="1">
            </element>

            <element name="oper_cashback_amount" type="tns:amount"
                minOccurs="0" maxOccurs="1">
            </element>

            <element name="sttl_amount" type="tns:amount" maxOccurs="1" minOccurs="0">
            </element>

            <element name="interchange_fee" type="tns:amount" minOccurs="0" maxOccurs="1">
            </element>

            <element name="originator_refnum" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <maxLength value="36"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="network_refnum" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <maxLength value="36"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="acq_inst_bin" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <maxLength value="12"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="response_code" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="8"/>
                        <maxLength value="8"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="oper_reason" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="8"/>
                        <maxLength value="8"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="status" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="8"/>
                        <maxLength value="8"/>
                        <enumeration value="OPST0001"/>
                        <enumeration value="OPST0100"/>
                        <enumeration value="OPST0101"/>
                        <enumeration value="OPST0102"/>
                        <enumeration value="OPST0103"/>
                        <enumeration value="OPST0107"/>
                        <enumeration value="OPST0110"/>
                        <enumeration value="OPST0120"/>
                        <enumeration value="OPST0130"/>
                        <enumeration value="OPST0200"/>
                        <enumeration value="OPST0300"/>
                        <enumeration value="OPST0400"/>
                        <enumeration value="OPST0401"/>
                        <enumeration value="OPST0402"/>
                        <enumeration value="OPST0403"/>
                        <enumeration value="OPST0404"/>
                        <enumeration value="OPST0500"/>
                        <enumeration value="OPST0501"/>
                        <enumeration value="OPST0600"/>
                        <enumeration value="OPST0700"/>
                        <enumeration value="OPST0800"/>
                        <enumeration value="OPST0850"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="status_reason" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="5"/>
                        <maxLength value="8"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="is_reversal" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="int">
                        <minInclusive value="0"/>
                        <maxInclusive value="1"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="merchant_number" minOccurs="0"
                maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="1"/>
                        <maxLength value="15"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="mcc" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="4"/>
                        <maxLength value="4"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="merchant_name" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="1"/>
                        <maxLength value="200"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="merchant_street" minOccurs="0"
                maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="1"/>
                        <maxLength value="200"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="merchant_city" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="1"/>
                        <maxLength value="200"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="merchant_region" minOccurs="0"
                maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="2"/>
                        <maxLength value="3"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="merchant_country" minOccurs="0"
                maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="3"/>
                        <maxLength value="3"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="merchant_postcode" minOccurs="0"
                maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="1"/>
                        <maxLength value="10"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="terminal_type" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="8"/>
                        <maxLength value="8"/>
                        <enumeration value="TRMT0001"/>
                        <enumeration value="TRMT0002"/>
                        <enumeration value="TRMT0003"/>
                        <enumeration value="TRMT0004"/>
                        <enumeration value="TRMT0005"/>
                        <enumeration value="TRMT0006"/>
                        <enumeration value="TRMT0007"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="terminal_number" minOccurs="0"
                maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="6"/>
                        <maxLength value="8"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="clearing_sequence_num" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="int">
                        <minInclusive value="0"/>
                        <maxInclusive value="99"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="clearing_sequence_count" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="int">
                        <minInclusive value="0"/>
                        <maxInclusive value="99"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="sttl_date" type="dateTime" minOccurs="0"
                maxOccurs="1">
            </element>

            <element name="acq_sttl_date" type="dateTime" minOccurs="0"
                maxOccurs="1">
            </element>

            <element name="match_id" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="long">
                        <minInclusive value="1"/>
                        <maxInclusive value="9999999999999999"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="payment_order" type="tns:payment_order"
                minOccurs="0" maxOccurs="1">
            </element>

            <element name="transaction" type="tns:transaction"
                minOccurs="0" maxOccurs="unbounded">
            </element>

            <element name="document" type="tns:document" minOccurs="0"
                maxOccurs="1">
            </element>

            <element name="issuer" type="tns:participant" minOccurs="0"
                maxOccurs="1">
            </element>

            <element name="acquirer" type="tns:participant"
                minOccurs="0" maxOccurs="1">
            </element>

            <element name="destination" type="tns:participant"
                minOccurs="0" maxOccurs="1">
            </element>

            <element name="aggregator" type="tns:participant"
                minOccurs="0" maxOccurs="1">
            </element>

            <element name="service_provider" type="tns:participant"
                minOccurs="0" maxOccurs="1">
            </element>

            <element name="participant" type="tns:participant"
                minOccurs="0" maxOccurs="unbounded">
            </element>

            <element name="note" type="tns:note" minOccurs="0"
                maxOccurs="unbounded">
            </element>

            <element name="auth_data" type="tns:auth_data" minOccurs="0"
                maxOccurs="1">
            </element>

            <element name="ipm_data" type="tns:ipm_data" minOccurs="0"
                maxOccurs="1">
            </element>

            <element name="baseII_data" type="tns:baseII_data"
                minOccurs="0" maxOccurs="1">
            </element>

            <element name="match_status" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="8"/>
                        <maxLength value="8"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="additional_amount" type="tns:additional_amount" minOccurs="0" maxOccurs="unbounded"/>
            
            <element name="processing_stage" type="tns:processing_stage" minOccurs="0" maxOccurs="unbounded"/>
            
            <element name="flexible_data" type="tns:flexible_data" minOccurs="0" maxOccurs="unbounded"/>
        </sequence>
    </complexType>

    <complexType name="participant">
        <sequence>
            <element name="participant_type" type="tns:dict" minOccurs="0" maxOccurs="1"/>

            <element name="client_id_type" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="8"/>
                        <maxLength value="8"/>
                        <enumeration value="CITPACCT"/> <!-- Identification by account -->
                        <enumeration value="CITPCARD"/> <!-- Identification by card number (PAN) -->
                        <enumeration value="CITPCDID"/> <!-- Identification by card's internal identifier -->
                        <enumeration value="CITPCASH"/> <!-- No identification, but cash is present -->
                        <enumeration value="CITPCMCH"/> <!-- Identification by card mask and card holder pair -->
                        <enumeration value="CITPCMPH"/> <!-- Identification by card mask and mobile phone number pair -->
                        <enumeration value="CITPCNTR"/> <!-- Identification by contract number -->
                        <enumeration value="CITPCUST"/> <!-- Identification by customer number -->
                        <enumeration value="CITPEMAI"/> <!-- Identification by e-mail -->
                        <enumeration value="CITPMBPH"/> <!-- Identification by mobile phone number -->
                        <enumeration value="CITPMRCT"/> <!-- Identification by merchant number -->
                        <enumeration value="CITPNONE"/> <!-- No identification -->
                        <enumeration value="CITPSRVP"/> <!-- Identification by service provider number -->
                        <enumeration value="CITPTRMN"/> <!-- Identification by terminal number -->
                        <enumeration value="CITPUNKN"/> <!-- Unknown identification -->
                        <enumeration value="CITPXCRD"/> <!-- Identification by linked card -->
                    </restriction>
                </simpleType>
            </element>

            <element name="client_id_value" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="2"/>
                        <maxLength value="200"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="card_number" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="11"/>
                        <maxLength value="19"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="card_id" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="long">
                        <minInclusive value="0"/>
                        <maxInclusive value="999999999999"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="card_instance_id" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="long">
                        <minInclusive value="0"/>
                        <maxInclusive value="999999999999"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="card_seq_number" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="int">
                        <minInclusive value="0"/>
                        <maxInclusive value="9999"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="card_expir_date" type="date" minOccurs="0" maxOccurs="1"/>

            <element name="card_country" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="3"/>
                        <maxLength value="3"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="inst_id" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="int">
                        <minInclusive value="1"/>
                        <maxInclusive value="9999"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="agent_number" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="1"/>
                        <maxLength value="200"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="network_id" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="int">
                        <minInclusive value="1000"/>
                        <maxInclusive value="9999"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="auth_code" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="6"/>
                        <maxLength value="6"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="account_number" type="tns:account_number" minOccurs="0" maxOccurs="1"/>

            <element name="account_amount" type="tns:amount_value" minOccurs="0" maxOccurs="1"/>

            <element name="account_currency" type="tns:currency" minOccurs="0" maxOccurs="1"/>

        </sequence>
    </complexType>

