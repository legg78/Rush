<?xml version="1.0" encoding="UTF-8"?>
<html	xmlns="http://www.w3.org/1999/xhtml"
		xmlns:f="http://java.sun.com/jsf/core"
		xmlns:h="http://java.sun.com/jsf/html"
		xmlns:ui="http://java.sun.com/jsf/facelets"
		xmlns:a4j="http://richfaces.org/a4j"
		xmlns:rich="http://richfaces.org/rich"
		xmlns:bm="http://www.bpc.ru/jsf/misc"
		xmlns:bre="http://www.bpc.ru/jsf/richfaces-ext">
<ui:composition>
	<ui:remove>
	<!-- 
		Just shows confirmation panel with predefined values usual for language change confirmation 
	-->
	</ui:remove>
	
    <ui:include src="/pages/utils/confirmPanel.jspx">
    	<ui:param name="backingBean" value="#{backingBean}"/>
    	<ui:param name="yesAction" value="#{empty yesAction ? 'confirmEditLanguage' : yesAction}"/>
    	<ui:param name="warningMsg" value="#{lblMsg.language_change_warning}"/>
    	<ui:param name="rerenderList" value="#{rerenderList}"/>
    	<ui:param name="editElem" value="#{editElem}"/>
    	<ui:param name="newValue" value="#{newValue}"/>
    </ui:include>
</ui:composition>
</html>
