<?xml version="1.0" encoding="UTF-8"?>
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core"
      xmlns:a4j="http://richfaces.org/a4j"
      xmlns:rich="http://richfaces.org/rich"
	  xmlns:bre="http://www.bpc.ru/jsf/richfaces-ext">
<ui:composition>
	
	<f:loadBundle var="lblAcq" basename="ru.bpc.sv2.ui.bundles.Acq"/>
	<f:loadBundle var="lblPrd" basename="ru.bpc.sv2.ui.bundles.Prd"/>
	<f:loadBundle var="lblAcc" basename="ru.bpc.sv2.ui.bundles.Acc" />
	<f:loadBundle var="lblFcl" basename="ru.bpc.sv2.ui.bundles.Fcl" />
	<f:loadBundle var="lblPmo" basename="ru.bpc.sv2.ui.bundles.Pmo" />
	<f:loadBundle var="lblRul" basename="ru.bpc.sv2.ui.bundles.Rul" />
	
	<h:form id="revenueSharingDetailsForm">
		<h:panelGroup id="revenueSharingDetailsWrapper" layout="block" styleClass="carddata-wrapper" 
				style="height: 100%; overflow-y: auto;overflow-x: hidden;position:relative;">
			<rich:dataTable id="revenueSharingDetailsTable"
					value="#{MbRevenueSharing.emptyTable}" 
					var="field"
					styleClass="res-table noHeader" width="100%">
				<rich:columnGroup rowClasses=",odd" 
						columnClasses="firstCol alignLeft width20, width80 width250 noPadding">
					<rich:column styleClass="detailsCategory expandedBranch">
						<h:graphicImage url="/images/minus.gif" onclick="showHideDetails(this)"/>
						<h:outputLabel value="#{lblPrd.general_data}:" />
					</rich:column>
					<rich:column>
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblAcq.terminal}:" title="#{lblAcq.terminal}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbRevenueSharing.activeRevenueSharing.terminalName}"
								title="#{MbRevenueSharing.activeRevenueSharing.terminalName}"/>
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblPrd.customer}:" title="#{lblPrd.customer}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbRevenueSharing.activeRevenueSharing.customerName}"
								title="#{MbRevenueSharing.activeRevenueSharing.customerName}" />
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblAcc.account}:" title="#{lblAcc.account}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbRevenueSharing.activeRevenueSharing.accountNumber}"
								title="#{MbRevenueSharing.activeRevenueSharing.accountNumber}"/>
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblFcl.fee_type}:" title="#{lblFcl.fee_type}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{DictUtils.articles[MbRevenueSharing.activeRevenueSharing.feeType]}"
								title="#{DictUtils.articles[MbRevenueSharing.activeRevenueSharing.feeType]}" />
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblFcl.fee}:" title="#{lblFcl.fee}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbRevenueSharing.activeRevenueSharing.feeDesc}"
								title="#{MbRevenueSharing.activeRevenueSharing.feeDesc}" />
					</rich:column>
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblPmo.provider}:" title="#{lblPmo.provider}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbRevenueSharing.activeRevenueSharing.providerId} - #{MbRevenueSharing.activeRevenueSharing.providerName}"
								title="#{MbRevenueSharing.activeRevenueSharing.providerId} - #{MbRevenueSharing.activeRevenueSharing.providerName}" />
					</rich:column>
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblPmo.purpose}:" title="#{lblPmo.purpose}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbRevenueSharing.activeRevenueSharing.purposeId} - #{MbRevenueSharing.activeRevenueSharing.purposeName}"
								title="#{MbRevenueSharing.activeRevenueSharing.purposeId} - #{MbRevenueSharing.activeRevenueSharing.purposeName}" />
					</rich:column>
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblRul.modifier}:" title="#{lblRul.modifier}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbRevenueSharing.activeRevenueSharing.modifierId} - #{MbRevenueSharing.activeRevenueSharing.modifierName}"
								title="#{MbRevenueSharing.activeRevenueSharing.modifierId} - #{MbRevenueSharing.activeRevenueSharing.modifierName}" />
					</rich:column>
				</rich:columnGroup>
			</rich:dataTable>
			<script>setDetailsState("revenueSharingDetailsForm:revenueSharingDetailsTable");</script>
		</h:panelGroup>
	</h:form>

</ui:composition>
</html>
