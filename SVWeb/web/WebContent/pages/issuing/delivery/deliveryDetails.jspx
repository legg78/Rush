                    </rich:column>
                    <rich:column>
                        <h:outputLabel value="#{MbDeliveries.activeDelivery.cardUID}"
                                title="#{MbDeliveries.activeDelivery.cardUID}"/>
                    </rich:column>

				</rich:columnGroup>
			</rich:dataTable>
			<script>setDetailsState("deliveriesDetailsForm:deliveriesDetailsTable");</script>
		</h:panelGroup>
	</h:form>
</ui:composition>
</html>