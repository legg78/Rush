<?xml version="1.0" encoding="UTF-8"?>
<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:a4j="http://richfaces.org/a4j"
	xmlns:rich="http://richfaces.org/rich"
	xmlns:bm="http://www.bpc.ru/jsf/misc"
	xmlns:bre="http://www.bpc.ru/jsf/richfaces-ext">
<ui:composition>
	<rich:modalPanel id="reissueReasonEditModalPanel" autosized="true"
		minWidth="300" minHeight="100">
		<f:facet name="header">
			<h:panelGroup id="reissueReasonEditModalPanelHeader">
				<h:outputText value="#{lblIss.edit_reiss_reason}"
					rendered="#{MbReissueReasonsSearch.editMode}" />
				<h:outputText value="#{lblIss.new_reiss_reason}"
					rendered="#{MbReissueReasonsSearch.newMode}" />
			</h:panelGroup>
		</f:facet>
		<f:facet name="controls">
		</f:facet>

		<h:form id="reissueReasonEditModalPanelForm">
		<script type="text/javascript">
		//<!--
				function checkFields() {
					var checked = true;
					
					if (document.getElementById('reissueReasonEditModalPanelForm:institution').value == "") {
						document.getElementById('reissueReasonEditModalPanelForm:institutionError').style.display = "inline";
						checked = false;
					}
					
					if (document.getElementById('reissueReasonEditModalPanelForm:reissueReason').value == "") {
						document.getElementById('reissueReasonEditModalPanelForm:reissueReasonError').style.display = "inline";
						checked = false;
					}
					
					if (document.getElementById('reissueReasonEditModalPanelForm:reissueCommand').value == "") {
						document.getElementById('reissueReasonEditModalPanelForm:reissueCommandError').style.display = "inline";
						checked = false;
					}
					
					if (document.getElementById('reissueReasonEditModalPanelForm:pinRequest').value == "") {
						document.getElementById('reissueReasonEditModalPanelForm:pinRequestError').style.display = "inline";
						checked = false;
					}
					
					if (document.getElementById('reissueReasonEditModalPanelForm:pinMailerRequest').value == "") {
						document.getElementById('reissueReasonEditModalPanelForm:pinMailerRequestError').style.display = "inline";
						checked = false;
					}
					
					if (document.getElementById('reissueReasonEditModalPanelForm:embossingRequest').value == "") {
						document.getElementById('reissueReasonEditModalPanelForm:embossingRequestError').style.display = "inline";
						checked = false;
					}
					return checked;
				}
				//-->
			</script>
			<h:panelGroup layout="block" styleClass="cardholderdata-wrapper">
				<h:panelGrid columns="2" styleClass="cardholderdata" columnClasses="firstColModal, width250">
					<h:outputLabel for="institution" title="#{lblCommon.institution}"
									 	value="#{MbReissueReasonsSearch.newMode ? '* ' : ''}#{lblCommon.institution}:"/>
					<h:panelGrid columns="1" cellpadding="0" cellspacing="0">
						<h:selectOneMenu id="institution"
							label="#{lblCommon.institution}"
							value="#{MbReissueReasonsSearch.editingItem.instId}"
							onblur="hideErrorField(this)"
							disabled="#{!MbReissueReasonsSearch.newMode}"
							readonly="#{!MbReissueReasonsSearch.newMode}">
							<f:selectItem itemValue="" />
							<f:selectItems value="#{MbReissueReasonsSearch.institutions}" />
						</h:selectOneMenu>
						<h:outputFormat value="#{lblMsg.value_request}" styleClass="errorMarker"
							id="institutionError" style="display:none;">
							<f:param name="fieldName" value="#{lblCommon.institution}"/>
						</h:outputFormat>
					</h:panelGrid>

					<h:outputLabel for="reissueReason" title="#{lblCommon.event_type}"
									 	value="#{MbReissueReasonsSearch.newMode ? '* ' : ''}#{lblCommon.event_type}:" />
					<h:panelGrid columns="1" cellpadding="0" cellspacing="0">
						<h:selectOneMenu id="reissueReason"
							label="#{lblCommon.event_type}"
							value="#{MbReissueReasonsSearch.editingItem.reissueReason}"
							onblur="hideErrorField(this)"
							disabled="#{!MbReissueReasonsSearch.newMode}"
							readonly="#{!MbReissueReasonsSearch.newMode}">
							<f:selectItem itemValue="" />
							<f:selectItems value="#{MbReissueReasonsSearch.eventTypes}" />
						</h:selectOneMenu>
						<h:outputFormat value="#{lblMsg.value_request}" styleClass="errorMarker"
							id="reissueReasonError" style="display:none;">
							<f:param name="fieldName" value="#{lblCommon.event_type}"/>
						</h:outputFormat>
					</h:panelGrid>

					<h:outputLabel for="reissueCommand" title="#{lblIss.reiss_command}"
									 	value="* #{lblIss.reiss_command}:" />
					<h:panelGrid columns="1" cellpadding="0" cellspacing="0">
						<h:selectOneMenu id="reissueCommand"
							label="#{lblIss.reiss_command}"
							value="#{MbReissueReasonsSearch.editingItem.reissueCommand}"
							onblur="hideErrorField(this)">
							<f:selectItem itemValue="" />
							<f:selectItems value="#{MbReissueReasonsSearch.reissueCommands}" />
						</h:selectOneMenu>
						<h:outputFormat value="#{lblMsg.value_request}" styleClass="errorMarker"
							id="reissueCommandError" style="display:none;">
							<f:param name="fieldName" value="#{lblIss.reiss_command}"/>
						</h:outputFormat>
					</h:panelGrid>

					<h:outputLabel for="pinRequest" title="#{lblIss.pin_request}"
									 	value="* #{lblIss.pin_request}:" />
					<h:panelGrid columns="1" cellpadding="0" cellspacing="0">
						<h:selectOneMenu id="pinRequest"
							label="#{lblIss.pin_request}"
							value="#{MbReissueReasonsSearch.editingItem.pinRequest}"
							onblur="hideErrorField(this)">
							<f:selectItem itemValue="" />
							<f:selectItems value="#{MbReissueReasonsSearch.pinRequests}" />
						</h:selectOneMenu>
						<h:outputFormat value="#{lblMsg.value_request}" styleClass="errorMarker"
							id="pinRequestError" style="display:none;">
							<f:param name="fieldName" value="#{lblIss.pin_request}"/>
						</h:outputFormat>
					</h:panelGrid>

					<h:outputLabel for="pinMailerRequest" title="#{lblIss.pin_mailer_request}"
									 	value="* #{lblIss.pin_mailer_request}:" />
					<h:panelGrid columns="1" cellpadding="0" cellspacing="0">
						<h:selectOneMenu id="pinMailerRequest"
							label="#{lblIss.pin_mailer_request}"
							value="#{MbReissueReasonsSearch.editingItem.pinMailerRequest}"
							onblur="hideErrorField(this)">
							<f:selectItem itemValue="" />
							<f:selectItems value="#{MbReissueReasonsSearch.pinMailerRequests}" />
						</h:selectOneMenu>
						<h:outputFormat value="#{lblMsg.value_request}" styleClass="errorMarker"
							id="pinMailerRequestError" style="display:none;">
							<f:param name="fieldName" value="#{lblIss.pin_mailer_request}"/>
						</h:outputFormat>
					</h:panelGrid>

					<h:outputLabel for="embossingRequest" title="#{lblIss.embossing_request}"
									 	value="* #{lblIss.embossing_request}:" />
					<h:panelGrid columns="1" cellpadding="0" cellspacing="0">
						<h:selectOneMenu id="embossingRequest"
							label="#{lblIss.embossing_request}"
							value="#{MbReissueReasonsSearch.editingItem.embossingRequest}"
							onblur="hideErrorField(this)">
							<f:selectItem itemValue="" />
							<f:selectItems value="#{MbReissueReasonsSearch.embossingRequests}" />
						</h:selectOneMenu>
						<h:outputFormat value="#{lblMsg.value_request}" styleClass="errorMarker"
							id="embossingRequestError" style="display:none;">
							<f:param name="fieldName" value="#{lblIss.embossing_request}"/>
						</h:outputFormat>
					</h:panelGrid>

					<h:outputLabel for="startDateRule" title="#{lblIss.reiss_start_date}"
									 	value="#{lblIss.reiss_start_date}:" />
					<h:selectOneMenu id="startDateRule"
						label="#{lblIss.reiss_start_date}"
						value="#{MbReissueReasonsSearch.editingItem.reissStartDateRule}"
						onblur="hideErrorField(this)">
						<f:selectItem itemValue="" />
						<f:selectItems value="#{MbReissueReasonsSearch.reissStartDateRules}" />
					</h:selectOneMenu>

					<h:outputLabel for="expirDateRule" title="#{lblIss.reiss_expir_date}"
									 	value="#{lblIss.reiss_expir_date}:" />
					<h:selectOneMenu id="expirDateRule"
						label="#{lblIss.reiss_expir_date}"
						value="#{MbReissueReasonsSearch.editingItem.reissExpirDateRule}"
						onblur="hideErrorField(this)">
						<f:selectItem itemValue="" />
						<f:selectItems value="#{MbReissueReasonsSearch.reissExpirDateRules}" />
					</h:selectOneMenu>

					<h:outputLabel for="persoPriority" title="#{lblIss.perso_priority}"
										value="#{lblIss.perso_priority}:" />
					<h:selectOneMenu id="persoPriority"
						label="#{lblIss.perso_priority}"
						value="#{MbReissueReasonsSearch.editingItem.persoPriority}"
						onblur="hideErrorField(this)">
						<f:selectItem itemValue="" />
						<f:selectItems value="#{MbReissueReasonsSearch.persoPriorities}" />
					</h:selectOneMenu>

					<h:outputLabel for="cloneOptionalServices" title="#{lblIss.reissue_clone_optional_services}"
								   value="#{lblIss.reissue_clone_optional_services}:"/>
					<h:selectOneMenu id="cloneOptionalServices"
									 label="#{lblIss.reissue_clone_optional_services}"
									 value="#{MbReissueReasonsSearch.editingItem.cloneOptionalServicesValue}"
									 onblur="hideErrorField(this)">
						<f:selectItem itemValue="" />
						<f:selectItems value="#{MbReissueReasonsSearch.cloneOptionalServices}" />
					</h:selectOneMenu>
				</h:panelGrid>
			</h:panelGroup>

			<h:panelGroup layout="block"
				styleClass="bottom_search_result_block_buttons">
				<h:panelGroup layout="block"
					styleClass="bottom_search_result_left_buttons">
					<h:panelGrid columns="3" columnClasses="width99P,,paddingR12">
						<h:outputText />
						<bre:taggedCommandButton value="#{lblForm.save}" type="submit"
							action="#{MbReissueReasonsSearch.save}"							
							onclick=" if (!checkFields()) return false;"
							oncomplete="if (#{usession.noErrorResponse})
								{#{rich:component('reissueReasonEditModalPanel')}.hide();
			                   			updateHeight();}"
							reRender="reissueReasonEditModalPanelForm, issReissueReasonsTable, issReissueReasonsScroller, pagesNum, btnPanel, detailsTab" />
							
						<bre:taggedCommandButton value="#{lblForm.cancel}"
							action="#{MbReissueReasonsSearch.cancel}" immediate="true"
							limitToList="true"
							oncomplete="#{rich:component('reissueReasonEditModalPanel')}.hide();
										updateHeight();"
							reRender="reissueReasonEditModalPanelForm">
							<f:actionListener type="ru.bpc.jsf.A4JFormReset" />						
						</bre:taggedCommandButton>
					</h:panelGrid>
				</h:panelGroup>
			</h:panelGroup>
		</h:form>
	</rich:modalPanel>
</ui:composition>
</html>