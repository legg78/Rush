								<h:outputLabel value="#{lblAcc.dest_ent_type}: " title="#{lblAcc.dest_ent_type}"/>
								<h:panelGrid columns="1" cellspacing="0" cellpadding="0">
									<h:selectOneMenu id="creditDestEntityType"
											value="#{MbEntryTemplates.newEntryTemplatePair.creditDestEntityType}"
											disabled="#{!MbEntryTemplates.newEntryTemplatePair.editCredit or MbEntryTemplates.editMode and MbEntryTemplates.newEntryTemplatePair.creditDestEntityType!=null}">
