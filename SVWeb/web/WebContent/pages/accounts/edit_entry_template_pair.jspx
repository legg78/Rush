											<ui:param name="accTypeModifierBean" value="#{MbAccTypeModifier}"/>
											<ui:param name="subject" value="credit"/>
											<ui:param name="del" value="creditDelete"/>
										</ui:include>
									</h:panelGroup>
								</h:panelGrid>

							</h:panelGrid>

						</h:panelGrid>
				</h:panelGroup>

				<h:panelGroup layout="block" styleClass="bottom_search_result_block_buttons">
					<h:panelGroup layout="block" styleClass="bottom_search_result_left_buttons">
						<h:panelGrid columns="3" columnClasses="width99P,,paddingR12">
							<h:outputText/>

							<bre:taggedCommandButton id="saveBtn"
													 value="#{lblForm.save}"
													 action="#{MbEntryTemplates.save}"
													 oncomplete="enableModalPanelBtns(this);
													             if (#{usession.noErrorResponse})
													                 #{rich:component('entryTmplModalPanel')}.hide()"
													 reRender="#{rerenderList}"
													 onclick="if (!checkEntryTemplatesFields()) return false;
													          disableModalPanelBtns(this)"
													 type="submit"/>

							<bre:taggedCommandButton id="cancelBtn"
													 value="#{lblForm.cancel}"
													 action="#{MbEntryTemplates.close}"
													 oncomplete="#{rich:component('entryTmplModalPanel')}.hide()"
													 reRender="entryTmplModalDiv"
													 immediate="true">
								<f:actionListener type="ru.bpc.jsf.A4JFormReset" />
							</bre:taggedCommandButton>
			    		</h:panelGrid>
			    	</h:panelGroup>
			    </h:panelGroup>
			</h:form>
			<ui:include src="/pages/accounts/editAccTypeMod.jspx">
				<ui:param name="accTypeModifierEditBean" value="#{MbAccTypeModifier}"/>
				<ui:param name="subject" value="credit"/>
				<ui:param name="rerenderList" value="editEntryTplForm:credit:accTypeModTable, editEntryTplForm:credit:accTypeModBtn" />
			</ui:include>
			<ui:include src="/pages/accounts/editAccTypeMod.jspx">
				<ui:param name="accTypeModifierEditBean" value="#{MbDbtAccTypeModifier}"/>
				<ui:param name="subject" value="debit"/>
				<ui:param name="rerenderList" value="editEntryTplForm:debit:accTypeModTable, editEntryTplForm:debit:accTypeModBtn" />
			</ui:include>
			<ui:include src="/pages/utils/confirmPanel.jspx">
				<ui:param name="backingBean" value="#{MbDbtAccTypeModifier}"/>
				<ui:param name="yesAction" value="delete"/>
				<ui:param name="warningMsg" value="#{lblMsg.confirm_delete}"/> 
				<ui:param name="rerenderList" value="editEntryTplForm:debit:accTypeModTable, editEntryTplForm:debit:accTypeModBtn"/>
				<ui:param name="confirmQuestion" value=" "/> 
				<ui:param name="yes" value="#{lblForm.ok}"/>
				<ui:param name="no" value="#{lblForm.cancel}"/>
				<ui:param name="subviewId" value="debitDelete"/>
			</ui:include>
			<ui:include src="/pages/utils/confirmPanel.jspx">
				<ui:param name="backingBean" value="#{MbAccTypeModifier}"/>
