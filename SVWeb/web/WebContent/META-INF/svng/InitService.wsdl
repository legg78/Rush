<?xml version="1.0" encoding="UTF-8"?>
<definitions name="sv_cup_wsdl"
			 xmlns="http://schemas.xmlsoap.org/wsdl/"
			 targetNamespace="http://www.bpcbt.com/prototype/interchange/service/v1/"
			 xmlns:tns="http://www.bpcbt.com/prototype/interchange/service/v1/"
			 xmlns:xsd="http://www.w3.org/2001/XMLSchema"
			 xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
			 xmlns:msg="http://www.bpcbt.com/prototype/interchange/message/v1/">

	<types>
		<schema xmlns="http://www.w3.org/2001/XMLSchema">
			<xsd:import namespace="http://www.bpcbt.com/prototype/interchange/message/v1/" schemaLocation="InitMessages.xsd"/>
		</schema>
	</types>

	<message name="InitRequest">
		<part name="request" element="msg:initRequest"/>
	</message>
	<message name="InitResponse">
		<part name="request" element="msg:initResponse"/>
	</message>

	<portType name="InitPortType">
		<operation name="Init">
			<input name="InitRequest" message="tns:InitRequest"/>
			<output name="InitResponse" message="tns:InitResponse"/>
		</operation>
	</portType>

	<binding name="InitBinding" type="tns:InitPortType">
		<soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
		<operation name="Init">
			<soap:operation soapAction="http://www.bpcbt.com/prototype/interchange/service/v1/Init"/>
			<input name="InitRequest">
				<soap:body use="literal" parts="request"/>
			</input>
			<output name="InitResponse">
				<soap:body use="literal"/>
			</output>
		</operation>
	</binding>

	<service name="InitService">
		<port name="InitPort" binding="tns:InitBinding">
			<soap:address location="http://localhost:7001/interchange"/>
		</port>
	</service>
</definitions>
