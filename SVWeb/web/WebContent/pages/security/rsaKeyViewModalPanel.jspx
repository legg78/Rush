<?xml version="1.0" encoding="UTF-8"?>
<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:a4j="http://richfaces.org/a4j"
	xmlns:rich="http://richfaces.org/rich"
	xmlns:bm="http://www.bpc.ru/jsf/misc"
	xmlns:bre="http://www.bpc.ru/jsf/richfaces-ext">
<ui:composition>

	<rich:modalPanel id="rsaKeyViewModalPanel" autosized="true"
		minWidth="300" minHeight="100">
		<f:facet name="header">
			<h:outputText value="#{lblSec.view_rsa_key}" />
		</f:facet>
		<f:facet name="controls">
		</f:facet>
		<h:form id="rsaKeyViewModalPanelForm">

			<h:panelGroup layout="block" styleClass="cardholderdata-wrapper">
				<h:panelGrid columns="2" styleClass="cardholderdata"
					columnClasses="firstCol, width250">

					<h:outputLabel value="#{lblSec.description}:" />
					<h:inputText label="#{lblSec.description}" readonly="true"
						value="#{MbRsaKey.activeItem.description}">
					</h:inputText>

					<h:outputLabel value="#{lblSec.lmk_id}:" />
					<h:inputText label="#{lblSec.lmk_id}" readonly="true"
						value="#{MbRsaKey.activeItem.lmkId}">
					</h:inputText>

					<h:outputLabel value="#{lblSec.hsm_device_id}:" rendered="false"/>
					<h:inputText readonly="true"
						value="#{MbRsaKey.hsmsMap[MbRsaKey.activeItem.hsmDeviceId]}"
						label="#{MbRsaKey.hsmsMap[MbRsaKey.activeItem.hsmDeviceId]}" rendered="false"/>

					<h:outputLabel value="#{lblSec.key_type}:" />
					<h:inputText readonly="true" label="#{lblSec.key_type}"
						value="#{DictUtils.articles[MbRsaKey.activeItem.keyType]}">
					</h:inputText>

					<h:outputLabel value="#{lblSec.key_index}:" />
					<h:inputText readonly="true" label="#{lblSec.key_index}"
						value="#{MbRsaKey.activeItem.keyIndex}">
					</h:inputText>

					<h:outputLabel value="#{lblSec.sign_algorithm}:" />
					<h:inputText readonly="true"
						value="#{DictUtils.articles[MbRsaKey.activeItem.signAlgorithm]}"
						label="#{DictUtils.articles[MbRsaKey.activeItem.signAlgorithm]}" />

					<h:outputLabel value="#{lblSec.modulus_length}:" />
					<h:inputText label="#{lblSec.modulus_length}" readonly="true"
						value="#{MbRsaKey.activeItem.modulusLength}">
					</h:inputText>

					<h:outputLabel value="#{lblSec.exponent}:" />
					<h:inputText readonly="true" label="#{lblSec.exponent}"
						value="#{MbRsaKey.activeItem.exponent}">
					</h:inputText>

					<h:outputLabel value="#{lblSec.public_key}:" />
					<h:inputText readonly="true" label="#{lblSec.public_key}"
						value="#{MbRsaKey.activeItem.publicKey}">
					</h:inputText>

					<h:outputLabel value="#{lblSec.private_key}:" />
					<h:inputText readonly="true" label="#{lblSec.private_key}"
						value="#{MbRsaKey.activeItem.privateKey}">
					</h:inputText>

					<h:outputLabel value="#{lblSec.private_key_mac}:" />
					<h:inputText readonly="true" label="#{lblSec.private_key_mac}"
						value="#{MbRsaKey.activeItem.publicKeyMac}">
					</h:inputText>

					<h:outputLabel value="#{lblSec.authority_id}:" />
					<h:inputText readonly="true" label="#{lblSec.authority_id}"
						value="#{MbRsaKey.authoritiesMap[MbRsaKey.activeItem.authorityId]}">
					</h:inputText>

					<h:outputLabel value="#{lblSec.subject_id}:" />
					<h:inputText readonly="true" label="#{lblSec.subject_id}"
						value="#{MbRsaKey.activeItem.subjectId}">
					</h:inputText>

					<h:outputLabel value="#{lblSec.expir_date}:" />
					<h:inputText readonly="true" label="#{lblSec.expir_date}"
						value="#{MbRsaKey.activeItem.expirDate}">
						<f:convertDateTime pattern="#{usession.datePattern}"
							timeZone="#{CommonUtils.timeZone}" />
					</h:inputText>

					<h:outputLabel value="#{lblSec.tracking_number}:" />
					<h:inputText label="#{lblSec.tracking_number}" readonly="true"
						value="#{MbRsaKey.activeItem.trackingNumber}">
					</h:inputText>

					<h:outputLabel value="#{lblSec.visa_service_id}:" />
					<h:inputText label="#{lblSec.visa_service_id}" readonly="true"
						value="#{MbRsaKey.activeItem.visaServiceId}">
					</h:inputText>
					
					<h:outputLabel value="#{lblSec.authority_key_index}:" />
					<h:panelGrid cellpadding="0" cellspacing="0">
						<h:panelGroup layout="block" style="width:200px;">
							<h:outputLabel value="Dec:" style="display:inline;padding:0 4px;" />
							<h:inputText style="width:50px" id="authorityKeyIndexDec"
								label="#{lblSec.authority_key_index}"
								value="#{MbRsaKey.activeItem.authorityKeyIndex}">
							</h:inputText>
						</h:panelGroup>
						<h:outputFormat value="#{lblSec.fill_only_one_field}" styleClass="errorMarker"
							id="errMultiSelect" style="display:none;">
							<f:param name="fieldName" value="#{lblSec.authority_key_index}" />
						</h:outputFormat>
					</h:panelGrid>
				</h:panelGrid>
			</h:panelGroup>

			<h:panelGroup layout="block"
				styleClass="bottom_search_result_block_buttons">
				<h:panelGroup layout="block"
					styleClass="bottom_search_result_left_buttons">
					<h:panelGrid columns="3" columnClasses="width99P,,paddingR12">

						<h:outputText />

						<h:outputText />

						<bre:taggedCommandButton value="#{lblForm.close}" type="submit"
							onclick="disableModalPanelBtns(this)"
							oncomplete="enableModalPanelBtns(this); if (#{usession.noErrorResponse}) 
			                   			#{rich:component('rsaKeyViewModalPanel')}.hide()" />

					</h:panelGrid>
				</h:panelGroup>
			</h:panelGroup>
		</h:form>
	</rich:modalPanel>
	
</ui:composition>
</html>