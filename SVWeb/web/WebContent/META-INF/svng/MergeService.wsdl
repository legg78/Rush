<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://schemas.xmlsoap.org/wsdl/"
			 targetNamespace="http://www.bpcbt.com/sv/merge/service/v1/"
			 xmlns:tns="http://www.bpcbt.com/sv/merge/service/v1/"
			 xmlns:xsd="http://www.w3.org/2001/XMLSchema"
			 xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
			 xmlns:msg="http://www.bpcbt.com/sv/merge/message/v1/">

	<types>
		<schema xmlns="http://www.w3.org/2001/XMLSchema">
			<xsd:import namespace="http://www.bpcbt.com/sv/merge/message/v1/" schemaLocation="MergeMessages.xsd"/>
		</schema>
	</types>

	<message name="MergeRequest">
		<part name="request" element="msg:mergeRequest"/>
	</message>
	<message name="ResultResponse">
		<part name="response" element="msg:resultResponse"/>
	</message>

	<portType name="MergePortType">
		<operation name="merge">
			<input name="MergeRequest" message="tns:MergeRequest"/>
			<output name="ResultResponse" message="tns:ResultResponse"/>
		</operation>
	</portType>

	<binding name="MergeBinding" type="tns:MergePortType">
		<soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
		<operation name="merge">
			<soap:operation soapAction="http://www.bpcbt.com/sv/merge/service/v1/merge"/>
			<input name="MergeRequest">
				<soap:body use="literal"/>
			</input>
			<output name="ResultResponse">
				<soap:body use="literal"/>
			</output>
		</operation>
	</binding>

	<service name="MergeService">
		<port name="MergePort" binding="tns:MergeBinding">
			<soap:address location="http://localhost:7001/merge"/>
		</port>
	</service>
</definitions>
