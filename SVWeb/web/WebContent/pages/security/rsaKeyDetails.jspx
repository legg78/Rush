<?xml version="1.0" encoding="UTF-8" ?>

<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:a4j="http://richfaces.org/a4j"
	xmlns:rich="http://richfaces.org/rich"
	xmlns:bpc="http://www.bpc.ru/jsf/misc">
<ui:composition>
	<f:loadBundle var="lblPrd" basename="ru.bpc.sv2.ui.bundles.Prd" />
	<h:form id="rsaKeyDetailsForm">
		<h:panelGroup layout="block" styleClass="langSelector">
			<h:panelGrid columns="2" styleClass="cardholderdata oneColumnTable"
					id="detailsData" columnClasses=",secondCol">
				<h:outputLabel for="lang" value="#{lblCommon.lang_selector}: "
						title="#{lblCommon.lang_selector}" />
				<h:selectOneMenu id="lang"
						disabled="#{MbRsaKey.activeItem.id == null}"
						value="#{MbRsaKey.curLang}"
						valueChangeListener="#{MbRsaKey.changeLanguage}">
					<a4j:support event="onchange" reRender="rsaKeyDetailsForm"
							limitToList="true" oncomplete="updateHeight();"/>
					<f:selectItems value="#{MbRsaKey.languages}" />
				</h:selectOneMenu>
			</h:panelGrid>
		</h:panelGroup>
	
		<h:panelGroup id="rsaKeyDetailsWrapper" layout="block" styleClass="carddata-wrapper" 
				style="height: 100%; overflow-y: auto;overflow-x: hidden;position:relative;">
			<rich:dataTable id="rsaKeyDetailsTable"
					value="#{MbRsaKey.emptyTable}" 
					var="field"
					styleClass="res-table noHeader" width="100%">
				<rich:columnGroup rowClasses=",odd" 
						columnClasses="firstCol alignLeft width20, width80 width250 noPadding">
					<rich:column styleClass="detailsCategory expandedBranch">
						<h:graphicImage url="/images/minus.gif" onclick="showHideDetails(this)"/>
						<h:outputLabel value="#{lblPrd.general_data}:" />
					</rich:column>
					<rich:column>
                        <script>updateHeight();</script>
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblSec.lmk_id}:" title="#{lblSec.lmk_id}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbRsaKey.activeItem.lmkId}"
								title="#{MbRsaKey.activeItem.lmkId}"/>
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblSec.key_type}:" title="#{lblSec.key_type}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel rendered="#{MbRsaKey.activeItem.keyType != null}"
								value="#{DictUtils.articles[MbRsaKey.activeItem.keyType]}"
								title="#{DictUtils.articles[MbRsaKey.activeItem.keyType]}" />
						<h:outputLabel value="" rendered="#{MbRsaKey.activeItem.keyType == null}" />
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblSec.key_index}:" title="#{lblSec.key_index}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbRsaKey.activeItem.keyIndex}"
								title="#{MbRsaKey.activeItem.keyIndex}">
							<bpc:hexadecimalConverter />
						</h:outputLabel>
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblSec.expir_date}:" title="#{lblSec.expir_date}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbRsaKey.activeItem.expirDate}"
								title="#{MbRsaKey.activeItem.expirDate}" />
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblSec.sign_algorithm}:" title="#{lblSec.sign_algorithm}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel rendered="#{MbRsaKey.activeItem.signAlgorithm != null}"
								value="#{DictUtils.articles[MbRsaKey.activeItem.signAlgorithm]}"
								title="#{DictUtils.articles[MbRsaKey.activeItem.signAlgorithm]}" />
						<h:outputLabel value="" rendered="#{MbRsaKey.activeItem.signAlgorithm == null}" />
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblSec.modulus_length}:" title="#{lblSec.modulus_length}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbRsaKey.activeItem.modulusLength}"
								title="#{MbRsaKey.activeItem.modulusLength}" />
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblSec.exponent}:" title="#{lblSec.exponent}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbRsaKey.activeItem.exponent}"
								title="#{MbRsaKey.activeItem.exponent}"/>
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblSec.public_key}:" title="#{lblSec.public_key}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbRsaKey.activeItem.publicKey}"
								title="#{MbRsaKey.activeItem.publicKey}" />
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblSec.public_key_mac}:" title="#{lblSec.public_key_mac}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbRsaKey.activeItem.publicKeyMac}"
								title="#{MbRsaKey.activeItem.publicKeyMac}" />
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblSec.private_key}:" title="#{lblSec.private_key}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbRsaKey.activeItem.privateKey}"
								title="#{MbRsaKey.activeItem.privateKey}" />
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblSec.standard_key_type}:" title="#{lblSec.standard_key_type}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbRsaKey.activeItem.standardKeyType}"
								title="#{MbRsaKey.activeItem.standardKeyType}" />
					</rich:column>
				
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblSec.description}:" title="#{lblSec.description}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbRsaKey.activeItem.description}"
								title="#{MbRsaKey.activeItem.description}"
								style="white-space: normal" />
					</rich:column>
				</rich:columnGroup>
			</rich:dataTable>
			<script>setDetailsState("rsaKeyDetailsForm:rsaKeyDetailsTable");</script>
		</h:panelGroup>
	</h:form>
</ui:composition>
</html>
