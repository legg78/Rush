            <xs:element name="pdc_6" type="String32" minOccurs="0" maxOccurs="1"/>
            <xs:element name="pdc_7" type="String32" minOccurs="0" maxOccurs="1"/>
            <xs:element name="pdc_8" type="String32" minOccurs="0" maxOccurs="1"/>
            <xs:element name="pdc_9" type="String32" minOccurs="0" maxOccurs="1"/>
            <xs:element name="pdc_10" type="String32" minOccurs="0" maxOccurs="1"/>
            <xs:element name="pdc_11" type="String32" minOccurs="0" maxOccurs="1"/>
            <xs:element name="pdc_12" type="String32" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="EmvData">
        <xs:sequence>
            <xs:element name="tag_5f2a" type="xs:decimal" minOccurs="0" maxOccurs="1"/>
            <xs:element name="tag_5f34" type="xs:decimal" minOccurs="0" maxOccurs="1"/>
            <xs:element name="tag_71" type="String64" minOccurs="0" maxOccurs="1"/>
            <xs:element name="tag_72" type="String64" minOccurs="0" maxOccurs="1"/>
            <xs:element name="tag_82" type="String32" minOccurs="0" maxOccurs="1"/>
            <xs:element name="tag_84" type="String128" minOccurs="0" maxOccurs="1"/>
            <xs:element name="tag_8a" type="String128" minOccurs="0" maxOccurs="1"/>
            <xs:element name="tag_91" type="String128" minOccurs="0" maxOccurs="1"/>
