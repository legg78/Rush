                    </rich:column>
                    <rich:column>
                        <h:outputLabel value="#{MbDeliveries.activeDelivery.agentId}"
                                title="#{MbDeliveries.activeDelivery.agentId}"/>
                    </rich:column>

                    <rich:column breakBefore="true">
                        <h:outputLabel value="#{lblCommon.institution}:" title="#{lblCommon.institution}"/>
                    </rich:column>
                    <rich:column>
                        <h:outputLabel value="#{MbDeliveries.activeDelivery.instId}"
                                title="#{MbDeliveries.activeDelivery.instId}"/>
                    </rich:column>

                    <rich:column breakBefore="true">
                        <h:outputLabel value="#{lblIss.blank_type}:" title="#{lblIss.blank_type}"/>
                    </rich:column>
                    <rich:column>
                        <h:outputLabel value="#{MbDeliveries.activeDelivery.blankTypeId}"
                                title="#{MbDeliveries.activeDelivery.blankTypeId}"/>
                    </rich:column>

                    <rich:column breakBefore="true">
                        <h:outputLabel value="#{lblIss.delivery_channel}:" title="#{lblIss.delivery_channel}"/>
                    </rich:column>
                    <rich:column>
                        <h:outputLabel value="#{DictUtils.articles[MbDeliveries.activeDelivery.deliveryChannel]}"
                                title="#{DictUtils.articles[MbDeliveries.activeDelivery.deliveryChannel]}"/>
                    </rich:column>

                    <rich:column breakBefore="true">
                        <h:outputLabel value="#{lblIss.PRECEDING_CARD_INSTANCE}:" title="#{lblIss.PRECEDING_CARD_INSTANCE}"/>
                    </rich:column>
                    <rich:column>
                        <h:outputLabel value="#{MbDeliveries.activeDelivery.precedingCardInstanceId}"
                                title="#{MbDeliveries.activeDelivery.precedingCardInstanceId}"/>
                    </rich:column>

                    <rich:column breakBefore="true">
                        <h:outputLabel value="#{lblIss.REISSUE_REASON}:" title="#{lblIss.REISSUE_REASON}"/>
                    </rich:column>
                    <rich:column>
                        <h:outputLabel value="#{DictUtils.articles[MbDeliveries.activeDelivery.reissueReason]}"
                                title="#{DictUtils.articles[MbDeliveries.activeDelivery.reissueReason]}"/>
                    </rich:column>

                    <rich:column breakBefore="true">
                        <h:outputLabel value="#{lblIss.REISSUE_DATE}:" title="#{lblIss.REISSUE_DATE}"/>
                    </rich:column>
                    <rich:column>
                        <h:outputLabel value="#{MbDeliveries.activeDelivery.reissueDate}"
                                title="#{MbDeliveries.activeDelivery.reissueDate}">
                            <f:convertDateTime pattern="#{usession.datePattern}" timeZone="#{CommonUtils.timeZone}" />
                        </h:outputLabel>
                    </rich:column>

                    <rich:column breakBefore="true">
                        <h:outputLabel value="#{lblCommon.session_id}:" title="#{lblCommon.session_id}"/>
                    </rich:column>
                    <rich:column>
