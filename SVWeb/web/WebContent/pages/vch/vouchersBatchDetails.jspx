								title="#{MbVouchersBatches.activeItem.cardNetworkId} - #{MbVouchersBatches.activeItem.networkName}"
								rendered="#{MbVouchersBatches.activeItem.cardNetworkId != null}" />
						<h:outputLabel value="" rendered="#{MbVouchersBatches.activeItem.cardNetworkId == null}" />
					</rich:column>
				
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblVch.user_name}:" title="#{lblVch.user_name}"/>
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbVouchersBatches.activeItem.userName}"
								title="#{MbVouchersBatches.activeItem.userName}" />
