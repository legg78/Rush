                        </rich:column>

                        <rich:column id="p3600" label="#{'P3600'}" width="5%">
                            <f:facet name="header">
                                <h:outputText value="#{'P3600'}" title="#{'P3600'}" />
                            </f:facet>
                            <h:outputText value="#{addendum.p3600}" />
                        </rich:column>

                        <rich:column id="p36001" label="#{'P3600_1'}" width="5%">
                            <f:facet name="header">
                                <h:outputText value="#{'P3600_1'}" title="#{'P3600_1'}" />
                            </f:facet>
                            <h:outputText value="#{addendum.p36001}" />
                        </rich:column>

                        <rich:column id="p36002" label="#{'P3600_2'}" width="5%">
