<?xml version="1.0" encoding="UTF-8"?>
<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:bm="http://www.bpc.ru/jsf/misc"
	xmlns:a4j="http://richfaces.org/a4j"
	xmlns:rich="http://richfaces.org/rich"
	xmlns:bre="http://www.bpc.ru/jsf/richfaces-ext">

<ui:composition>
<f:loadBundle var="lblPrd" basename="ru.bpc.sv2.ui.bundles.Prd" />
	<h:form id="arrayTypeDetailsForm">
		<h:panelGroup layout="block" styleClass="langSelector">
			<h:panelGrid columns="2" styleClass="cardholderdata oneColumnTable"
					id="detailsData" columnClasses=",secondCol">
				<h:outputLabel for="lang" value="#{lblCommon.lang_selector}: "
						title="#{lblCommon.lang_selector}" />
				<h:selectOneMenu id="lang"
						disabled="#{MbArrayTypes.detailArrayType.id == null}"
						value="#{MbArrayTypes.detailArrayType.lang}"
						valueChangeListener="#{MbArrayTypes.changeLanguage}">
					<a4j:support event="onchange" reRender="arrayTypeDetailsForm"
							limitToList="true" oncomplete="updateHeight();"/>
					<f:selectItems value="#{MbArrayTypes.languages}" />
				</h:selectOneMenu>
			</h:panelGrid>
		</h:panelGroup>
		<h:panelGroup id="arrayTypeDetailsWrapper" layout="block" styleClass="carddata-wrapper" 
				style="height: 100%; overflow-y: auto;overflow-x: hidden;position:relative;">
			<rich:dataTable id="arrayTypeDetailsTable"
					value="#{MbArrayTypes.emptyTable}" 
					var="field"
					styleClass="res-table noHeader" width="100%">
				<rich:columnGroup rowClasses=",odd" 
						columnClasses="firstCol alignLeft width20, width80 width250 noPadding">
					<rich:column styleClass="detailsCategory expandedBranch">
						<h:graphicImage url="/images/minus.gif" onclick="showHideDetails(this)"/>
						<h:outputLabel value="#{lblPrd.general_data}:" />
					</rich:column>
					<rich:column>
						<script>updateHeight();</script>
					</rich:column>					
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblCommon.id}: " title="#{lblCommon.id}" />
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbArrayTypes.detailArrayType.id}"
								title="#{MbArrayTypes.detailArrayType.id}" />
					</rich:column>
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblCommon.system_name}: " title="#{lblCommon.system_name}" />
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbArrayTypes.detailArrayType.systemName}"
								title="#{MbArrayTypes.detailArrayType.systemName}" />
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblCommon.name}: " title="#{lblCommon.name}" />
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbArrayTypes.detailArrayType.name}"
								title="#{MbArrayTypes.detailArrayType.name}"/>
					</rich:column>
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblCommon.institution}: " title="#{lblCommon.institution}" />
					</rich:column>
					<rich:column>
						<bm:entityDisplay  entityId="#{MbArrayTypes.detailArrayType.instId}"
											value="#{MbArrayTypes.detailArrayType.instName}"/>
					</rich:column>				
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblCommon.entity_type}: " title="#{lblCommon.entity_type}" />
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{DictUtils.articles[MbArrayTypes.detailArrayType.entityType]}"
								title="#{DictUtils.articles[MbArrayTypes.detailArrayType.entityType]}"
								rendered="#{MbArrayTypes.detailArrayType.entityType != null}"/>
						<h:outputLabel value=""
								rendered="#{MbArrayTypes.detailArrayType.entityType == null}"/>					
					</rich:column>
	
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblCommon.data_type}: " title="#{lblCommon.data_type}" />
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{DictUtils.articles[MbArrayTypes.detailArrayType.dataType]}"
								title="#{DictUtils.articles[MbArrayTypes.detailArrayType.dataType]}"
								rendered="#{MbArrayTypes.detailArrayType.dataType != null}"/>
						<h:outputLabel value=""
								rendered="#{MbArrayTypes.detailArrayType.dataType == null}"/>	
					</rich:column>
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblCommon.lov}: " title="#{lblCommon.lov}" />
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbArrayTypes.detailArrayType.lovId} - #{MbArrayTypes.detailArrayType.lovName}"
								title="#{MbArrayTypes.detailArrayType.lovId} - #{MbArrayTypes.detailArrayType.lovName}"
								rendered="#{MbArrayTypes.detailArrayType.lovId != null}"/>
						<h:outputLabel value=""
								rendered="#{MbArrayTypes.detailArrayType.lovId == null}"/>	
					</rich:column>
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblCommon.unique}: " title="#{lblCommon.unique}" />
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbArrayTypes.detailArrayType.unique ? 'true' : 'false'}"
								title="#{MbArrayTypes.detailArrayType.unique ? 'true' : 'false'}"
								rendered="#{MbArrayTypes.detailArrayType.id != null}"/>
						<h:outputLabel value=""
								rendered="#{MbArrayTypes.detailArrayType.id == null}"/>
					</rich:column>
					
					<rich:column breakBefore="true">
						<h:outputLabel value="#{lblCommon.description}:" title="#{lblCommon.description}" />
					</rich:column>
					<rich:column>
						<h:outputLabel value="#{MbArrayTypes.detailArrayType.description}"
								title="#{MbArrayTypes.detailArrayType.description}" style="white-space: normal"/>
					</rich:column>

                    <rich:column breakBefore="true">
                        <h:outputLabel value="#{lblRul.scale_type}:" title="#{lblRul.scale_type}" />
                    </rich:column>
                    <rich:column>
                        <h:outputLabel value="#{DictUtils.articles[MbArrayTypes.detailArrayType.scaleType]}"
								title="#{DictUtils.articles[MbArrayTypes.detailArrayType.scaleType]}"
								rendered="#{MbArrayTypes.detailArrayType.scaleType != null}"/>
						<h:outputLabel value=""
								rendered="#{MbArrayTypes.detailArrayType.scaleType == null}"/>
                    </rich:column>

                    <rich:column breakBefore="true">
                        <h:outputLabel value="#{lblCommon.class_name}:" title="#{lblCommon.class_name}" />
                    </rich:column>
                    <rich:column>
                        <h:outputLabel value="#{MbArrayTypes.detailArrayType.className}"
                                       title="#{MbArrayTypes.detailArrayType.className}" style="white-space: normal"/>
                    </rich:column>

                </rich:columnGroup>
			</rich:dataTable>
			<script>setDetailsState("arrayTypeDetailsForm:arrayTypeDetailsTable");</script>
		</h:panelGroup>
	</h:form>
</ui:composition>
</html>
