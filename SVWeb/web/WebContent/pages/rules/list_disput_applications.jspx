                            </rich:column>

                            <rich:column id="dueDateColumn" label="#{lblCrd.due_date}" title="#{lblCrd.due_date}"
                                         sortBy="#{'due_date'}" width="6%"
                                         style="#{MbDspApplications.showDueDateAlarm(item.dueDate) ? 'background-color: #FF5555;' : ''}">
