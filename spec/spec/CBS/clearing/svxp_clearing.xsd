                    </restriction>
                </simpleType>
            </element>

            <element name="purpose_id" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="int">
                        <minInclusive value="0"/>
                        <maxInclusive value="99999999"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="purpose_number" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="1"/>
                        <maxLength value="200"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="payment_amount" type="tns:amount" minOccurs="0" maxOccurs="1"/>

            <element name="payment_date" type="dateTime" minOccurs="0" maxOccurs="1"/>

            <element name="payment_parameter" type="tns:payment_parameter" minOccurs="0" maxOccurs="unbounded"/>

            <element name="participant_type" type="tns:dict" minOccurs="0" maxOccurs="1"/>

            <element name="document" type="tns:document" minOccurs="0" maxOccurs="1"/>

        </sequence>
    </complexType>

    <complexType name="payment_parameter">
        <sequence>
            <element name="payment_parameter_name" minOccurs="1" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="1"/>
                        <maxLength value="200"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="payment_parameter_value" minOccurs="1" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="1"/>
                        <maxLength value="2000"/>
                    </restriction>
                </simpleType>
            </element>

        </sequence>
    </complexType>

    <complexType name="transaction">
        <sequence>

            <element name="transaction_id" minOccurs="1" maxOccurs="1">
                <simpleType>
                    <restriction base="long">
                        <minInclusive value="1"/>
                        <maxInclusive value="9999999999999999"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="transaction_type" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="8"/>
                        <maxLength value="8"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="posting_date" type="dateTime" minOccurs="0" maxOccurs="1"/>

            <element name="debit_entry" type="tns:entry" minOccurs="0" maxOccurs="1"/>

            <element name="credit_entry" type="tns:entry" minOccurs="0" maxOccurs="1"/>

            <element name="document" type="tns:document" minOccurs="0" maxOccurs="1"/>
            
            <element name="conversion_rate" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="float">
                    </restriction>
                </simpleType>
            </element>
            
            <element name="rate_type" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="8"/>
                        <maxLength value="8"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="amount_purpose" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="8"/>
                        <maxLength value="8"/>
                    </restriction>
                </simpleType>
            </element>

        </sequence>
    </complexType>

    <complexType name="entry">
        <sequence>

            <element name="entry_id" minOccurs="1" maxOccurs="1">
                <simpleType>
                    <restriction base="long">
                        <minInclusive value="1"/>
                        <maxInclusive value="9999999999999999"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="status" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="8"/>
                        <maxLength value="8"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="account" type="tns:account" minOccurs="1" maxOccurs="1"/>

            <element name="amount" type="tns:amount" minOccurs="1" maxOccurs="1"/>

        </sequence>
    </complexType>

    <complexType name="account">
        <sequence>
            <element name="account_number" type="tns:account_number" minOccurs="1" maxOccurs="1"/>
            <element name="currency" type="tns:currency" minOccurs="1" maxOccurs="1"/>

            <element name="balance_type" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="8"/>
                        <maxLength value="8"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="agent_number" minOccurs="0" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="1"/>
                        <maxLength value="200"/>
                    </restriction>
                </simpleType>
            </element>
        </sequence>
    </complexType>

    <complexType name="amount">
        <sequence>
            <element name="amount_value" type="tns:amount_value" minOccurs="1" maxOccurs="1"/>
            <element name="currency" type="tns:currency" minOccurs="1" maxOccurs="1"/>
        </sequence>
    </complexType>

    <complexType name="document">
        <sequence>

            <element name="document_id" minOccurs="1" maxOccurs="1">
                <simpleType>
                    <restriction base="long">
                        <minInclusive value="1"/>
                        <maxInclusive value="9999999999999999"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="document_type" minOccurs="1" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="8"/>
                        <maxLength value="8"/>
                        <enumeration value="DCMT0001"/> <!-- Scanned image -->
                        <enumeration value="DCMT0002"/> <!-- Report -->
                        <enumeration value="DCMT0003"/> <!-- Credit statement -->
                        <enumeration value="DCMT0004"/> <!-- Cardholder dispute form -->
                        <enumeration value="DCMT0005"/> <!-- Copy of sales slip -->
                        <enumeration value="DCMT0006"/> <!-- Copy of credit slip/voucher -->
                        <enumeration value="DCMT0007"/> <!-- Copy of return receipt -->
                        <enumeration value="DCMT0008"/> <!-- Copy of alternate payment -->
                        <enumeration value="DCMT0009"/> <!-- Copy of merchant's delivery terms and conditions -->
                        <enumeration value="DCMT0010"/> <!-- Correspondence with merchant -->
                        <enumeration value="DCMT0011"/> <!-- Other attachment regarding a dispute -->
                        <enumeration value="DCMT0012"/> <!-- Customer statement -->
                        <enumeration value="DCMT0013"/> <!-- Loyalty statement -->

                    </restriction>
                </simpleType>
            </element>

            <element name="document_date" type="date" minOccurs="1" maxOccurs="1"/>

            <element name="document_number" minOccurs="1" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="2"/>
                        <maxLength value="200"/>
                    </restriction>
                </simpleType>
            </element>

            <element name="document_content" type="tns:document_content" minOccurs="0" maxOccurs="unbounded"/>

        </sequence>
    </complexType>

    <complexType name="document_content">
        <sequence>

            <element name="content_type" minOccurs="1" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="8"/>
                        <maxLength value="8"/>
                        <enumeration value="DCCT0010"/> <!-- Printable form -->
                        <enumeration value="DCCT0011"/> <!-- Dispute attachment -->
                    </restriction>
                </simpleType>
            </element>

            <element name="content" type="base64Binary" minOccurs="1" maxOccurs="1"/>

        </sequence>
    </complexType>

    <complexType name="note">
        <sequence>
            <element name="note_type" minOccurs="1" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="8"/>
                        <maxLength value="8"/>
                        <enumeration value="NTTPUSER"/> <!-- User comment -->
                        <enumeration value="NTTPDLVR"/> <!-- Delivery notes. -->
                        <enumeration value="NTTPRSTR"/> <!-- Notes for registry of opened and closed accounts. -->
                    </restriction>
                </simpleType>
            </element>

            <element name="note_content" type="tns:note_content" minOccurs="1" maxOccurs="unbounded"/>

        </sequence>
    </complexType>

    <complexType name="note_content">
        <sequence>

            <element name="note_header" type="string" minOccurs="0" maxOccurs="1"/>

            <element name="note_text" type="string" minOccurs="0" maxOccurs="1"/>

        </sequence>

        <attribute name="language" use="required">
            <simpleType>
                <restriction base="string">
                    <minLength value="5"/>
                    <maxLength value="8"/>
                    <enumeration value="LANGBLR"><!-- Belorussian --></enumeration>
                    <enumeration value="LANGBUL"><!-- Bulgarian   --></enumeration>
                    <enumeration value="LANGENG"><!-- English     --></enumeration>
                    <enumeration value="LANGESP"><!-- Spanish     --></enumeration>
                    <enumeration value="LANGFRA"><!-- French      --></enumeration>
                    <enumeration value="LANGHUN"><!-- Hungarian   --></enumeration>
                    <enumeration value="LANGKOR"><!-- Korean      --></enumeration>
                    <enumeration value="LANGMNG"><!-- Mongolian   --></enumeration>
                    <enumeration value="LANGRUS"><!-- Russian     --></enumeration>
                    <enumeration value="LANGTJK"><!-- Tajik       --></enumeration>
                    <enumeration value="LANGVNM"><!-- Vietnamese  --></enumeration>
                </restriction>
            </simpleType>
        </attribute>

    </complexType>

    <complexType name="auth_tag">
        <choice>
            <sequence>
                <element name="tag_id" minOccurs="1" maxOccurs="1" type="tns:intId"/>
                <element name="tag_value" minOccurs="1" maxOccurs="1" type="tns:stringValue"/>
                <element name="tag_name" minOccurs="0" maxOccurs="1" type="tns:stringName"/>
                <element name="seq_number" minOccurs="0" maxOccurs="1" type="tns:intId"/>
            </sequence>
            <sequence>
                <element name="tag_name" minOccurs="1" maxOccurs="1" type="tns:stringName"/>
                <element name="tag_value" minOccurs="1" maxOccurs="1" type="tns:stringValue"/>
                <element name="tag_id" minOccurs="0" maxOccurs="1" type="tns:intId"/>
                <element name="seq_number" minOccurs="0" maxOccurs="1" type="tns:intId"/>
            </sequence>
        </choice>
    </complexType>

    <complexType name="additional_amount">
        <sequence>
            <element name="amount_value" type="tns:sign_amount_value" minOccurs="1" maxOccurs="1"/>
            <element name="currency" type="tns:currency" minOccurs="1" maxOccurs="1"/>
            <element name="amount_type" type="tns:dict" minOccurs="1" maxOccurs="1"/>
        </sequence>
    </complexType>

    <complexType name="processing_stage">
        <sequence>
            <element name="proc_stage" minOccurs="1" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="5"/>
                        <maxLength value="8"/>
                        <pattern value="PSTG[A-F0-9]{1,4}"/> <!-- Processing stages -->
                    </restriction>
                </simpleType>
            </element>
            <element name="status" minOccurs="1" maxOccurs="1">
                <simpleType>
                    <restriction base="string">
                        <minLength value="5"/>
                        <maxLength value="8"/>
                        <pattern value="OPST[A-F0-9]{1,4}"/> <!-- Operation statuses -->
                    </restriction>
                </simpleType>
            </element>
        </sequence>
    </complexType>

    <complexType name="flexible_data">
        <sequence>
            <element name="field_name" type="tns:string_200_type" minOccurs="1" maxOccurs="1"/>
            <element name="field_value" type="tns:string_200_type" minOccurs="1" maxOccurs="1"/>
        </sequence>
    </complexType>
    
    <element name="clearing" type="tns:clearing"/>
</schema>


